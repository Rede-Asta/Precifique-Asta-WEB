\n        }));\n      }\n    }\n  },\n\n  data() {\n    return {\n      showButtons: false,\n      canScrollLeft: false,\n      canScrollRight: false,\n    }\n  },\n\n  mounted() {\n    window.addEventListener('resize', this.hideOrShowButtons);\n\n    this.$nextTick(() => {\n      this.hideOrShowButtons();\n      this.updateCanScroll();\n    });\n  },\n\n  beforeDestroy() {\n    window.removeEventListener('resize', this.hideOrShowButtons);\n  },\n\n  methods: {\n    hideOrShowButtons() {\n      this.showButtons = this.isSliderOverflowing();\n    },\n\n    next() {\n      const slider = this.$refs.slider as HTMLDivElement;\n      if (slider) {\n        slider.scrollLeft = slider.scrollLeft + (slider.offsetWidth / 2);\n      }\n      this.updateCanScroll();\n    },\n\n    previous() {\n      const slider = this.$refs.slider as HTMLDivElement;\n      if (slider) {\n        slider.scrollLeft = slider.scrollLeft - (slider.offsetWidth / 2);\n      }\n      this.updateCanScroll();\n    },\n\n    updateCanScroll() {\n      const slider = this.$refs.slider as HTMLDivElement;\n      if (this.showButtons && slider) {\n        this.canScrollLeft = slider.scrollLeft !== 0;\n        this.canScrollRight = slider.scrollWidth - slider.scrollLeft - slider.clientWidth > 1;\n      }\n    },\n\n    isSliderOverflowing() {\n      const slider = this.$refs.slider as HTMLDivElement;\n      if (slider) {\n        return slider.scrollWidth > slider.clientWidth;\n      }\n      return false;\n    }\n  }\n});\n"],"sourceRoot":""}]}