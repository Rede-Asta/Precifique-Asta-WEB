ics.vue';\nimport ProductCreateView from '@/views/Product/ProductCreateView.vue';\nimport ProductEditView from '@/views/Product/ProductEditView.vue';\nimport HomeSeller from '@/views/home-seller/home-seller.vue';\n\n\nVue.use(VueRouter);\nconst orderService = new OrderService();\nconst registerService = new RegisterSellerService();\n\nconst routes: RouteConfig[] = [\n  {\n    path: '/login',\n    name: 'Login',\n    component: Login,\n    meta: {\n      title: 'Login',\n    },\n  },\n  {\n    path: '/quero-vender',\n    name: 'Quero Vender',\n    component: RegisterSeller,\n    meta: {\n      title: 'Quero Vender',\n    },\n  },\n  {\n    path: '/',\n    redirect: '/home-seller',\n  },\n  {\n    path: '/home-seller',\n    component: HomeSeller,\n  },\n  {\n    path: '/dashboard',\n    name: 'Dashboard',\n    component: dashboardView,\n    meta: {\n      title: 'Dashboard',\n      requiresAuth: true,\n    },\n  },\n  {\n    path: '/home',\n    name: 'Home',\n    component: Home,\n    meta: {\n      title: 'Home',\n    },\n  },\n  {\n    path: '/pedidos',\n    name: 'Pedidos',\n    component: Pedidos,\n    meta: {\n      title: 'Pedidos',\n      requiresAuth: true,\n    },\n  },\n  {\n    path: '/produtos',\n    name: 'produtos',\n    component: Produtos,\n    meta: {\n      title: 'Produtos',\n      requiresAuth: true,\n    },\n  },\n  {\n    path: '/produtos/criar',\n    name: 'ProductCreate',\n    component: ProductCreateView,\n    meta: {\n      title: 'Criar Produto',\n      requiresAuth: true,\n    }\n  },\n  {\n    path: '/produtos/editar/:productGuid/:type?',\n    name: 'ProductEdit',\n    component: ProductEditView,\n    meta: {\n      title: 'Editar Produto',\n      requiresAuth: true,\n    }\n  },\n  {\n    path: '/produtos/criar/:type/:categoryGuid?',\n    name: 'ProductCreate',\n    component: ProductCreateView,\n    meta: {\n      title: 'Criar Produto',\n      requiresAuth: true,\n    }\n  },\n  {\n    path: '/edit',\n    redirect: '/minha-loja',\n  },\n  {\n    path: '/minha-loja',\n    name: 'Edit',\n    component: editiew,\n    meta: {\n      title: 'Editar',\n      requiresAuth: true,\n    },\n  },\n  {\n    path: '/meus-pedidos/:guid',\n    name: 'Order Tracking',\n    component: orderTracking,\n    meta: {\n      title: 'Acompanhamento do Pedido',\n      requiresAuth: true,\n      pageTitle: 'Acompanhamento do Pedido',\n    },\n  },\n  {\n    path: '/cursos',\n    name: 'Cursos',\n    component: coursesView,\n    meta: {\n      title: 'Cursos',\n      requiresAuth: false,\n      pageTitle: 'Pertinho de Casa - Educacional',\n    },\n  },\n  {\n    path: '/estatisticas',\n    name: 'statistics',\n    component: StatisticsView,\n    meta: {\n      title: 'Estatísticas',\n      requiresAuth: true,\n      pageTitle: 'Pertinho de Casa - Estatísticas',\n    },\n  }\n];\n\nconst router = new VueRouter({\n  mode: 'history',\n  base: process.env.BASE_URL,\n  routes,\n  scrollBehavior(to, from, savedPosition) {\n    if (to.name === 'ProductEdit' || to.name === 'ProductCreate') {\n      return { x: 0, y: 0 } as any;\n    }\n\n    return savedPosition\n  }\n});\n\nrouter.beforeEach(async (to, from, next) => {\n  if (to.name === 'Order Tracking') {\n    orderService.setSessionStorageOrderTracking(to.params.guid);\n  }\n\n  Boolean(to.query.dev)\n    ? localStorage.setItem('developer', JSON.stringify(true))\n    : localStorage.setItem('developer', JSON.stringify(false));\n\n  const tokenUser = UserStorage.returnToken();\n  const response = await registerService.validateTokenSeller();\n\n  document.title = to.meta && to.meta.pageTitle || 'Pertinho de Casa - Admin';\n\n  if (to.matched.some(record => record.meta.requiresAuth)) {\n    if (tokenUser !== null && response.ok) {\n      return next();\n    } else {\n      UserStorage.clearData();\n      return next({\n        path: '/login',\n      });\n    }\n  } else {\n    return next();\n  }\n});\n\nexport default router;\n"],"sourceRoot":""}]}