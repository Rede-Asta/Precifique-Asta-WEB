S,CAAC,EAAD,CACa,CAAA,EAAV,UAAU,CAAV;SAAA,U;AAqFb,SAAS,UAAU,IAAI,OAAvB","sourcesContent":["\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nimport { Component, Prop, Vue } from \"vue-property-decorator\";\n\ndeclare global {\n  interface Window {\n    google: any;\n  }\n}\n\n@Component({})\nexport class MapAddress extends Vue {\n  @Prop() propShowMapAddress!: boolean;\n\n  public title: string = \"Confirme seu endereço\";\n  public subtitle: string =\n    \"Clique no ponto do mapa mais próximo à sua localização e confirme seu endereço.\";\n\n  public showMapAddress: boolean = this.propShowMapAddress;\n\n  mounted() {\n    this.initMap();\n  }\n\n  private initMap() {\n    let myLatlng = JSON.parse(localStorage.getItem(\"latlng\") || \"\");\n    const map = new window.google.maps.Map(this.$refs.map, {\n      center: myLatlng,\n      zoom: 17,\n      mapTypeControl: false,\n      scaleControl: false,\n      streetViewControl: false,\n      rotateControl: false,\n      fullscreenControl: false,\n      overviewMapControl: false,\n    });\n    let infoWindow = this.placeInfoWindow(myLatlng);\n    let markerUser = this.placeMarkerUser(myLatlng, map);\n\n    const setMarkerPos = (myLatlng) => {\n      let center = map.getCenter();\n      myLatlng = { lat: center.lat(), lng: center.lng() };\n      localStorage.setItem(\"latlng\", JSON.stringify(myLatlng));\n      markerUser.setPosition(center);\n      infoWindow.setPosition(center);\n    };\n\n    map.addListener(\"drag\", (event) => {\n      setMarkerPos(myLatlng);\n    });\n\n    map.addListener(\"dblclick\", (event) => {\n      setMarkerPos(myLatlng);\n    });\n\n    map.addListener(\"zoom_changed\", (event) => {\n      setMarkerPos(myLatlng);\n    });\n\n    infoWindow.open(map);\n  }\n\n  private placeInfoWindow(myLatlng: any) {\n    return new window.google.maps.InfoWindow({\n      content: `\n                <h6 class=\"text-center font-weight-bold m-0\">Esta é localização do negócio?</h6>\n                <p class=\"text-center m-0 text-muted\">Se for necessário ajuste sua localização</p>\n            `,\n      position: myLatlng,\n    });\n  }\n\n  private placeMarkerUser(myLatlng: any, map) {\n    return new window.google.maps.Marker({\n      position: myLatlng,\n      map,\n      title: \"Estou aqui\",\n      icon: {\n        url: require(\"@/assets/images/icon-place.svg\"),\n        size: new window.google.maps.Size(50, 50),\n        origin: new window.google.maps.Point(0, 0),\n        anchor: new window.google.maps.Point(24, 10),\n        scaledSize: new window.google.maps.Size(50, 50),\n      },\n    });\n  }\n\n  public confirmAddressMap() {\n    this.$emit(\"confirmAddressMap\", true);\n  }\n\n  public closeMapAddress() {\n    this.$emit(\"closeMapAddress\", true);\n  }\n}\n\nexport { MapAddress as default };\n"],"sourceRoot":""}]}