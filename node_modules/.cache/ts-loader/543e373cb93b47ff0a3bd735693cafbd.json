AqB,CAAA,E,yBAAA,E,QAAA,E,KAAiB,CAAjB,CAAA;;AACH,UAAA,CAAA,CAAtB,IAAI,CAAC;AAAE,EAAA,OAAO,EAAE;AAAX,CAAD,CAAkB,CAAA,E,yBAAA,E,KAAA,E,KAAa,CAAb,CAAA;;AAJd,eAAe,GAAA,UAAA,CAAA,CAD3B,SAAS,CAAC,EAAD,CACkB,CAAA,EAAf,eAAe,CAAf;SAAA,e","sourcesContent":["import { Component, Prop, Vue } from 'vue-property-decorator';\nimport Swal, { SweetAlertType } from 'sweetalert2';\nimport VueSimpleAlert           from 'vue-simple-alert';\n\nimport Seller from '@/models/seller';\n\nimport getUtm from '@/helpers/getUtm';\nimport environment from \"@/environments/environment\";\n\n@Component({})\nexport class headerComponent extends Vue {\n\n\n    @Prop({ default: false }) voltar!: boolean;\n    @Prop({ default: '' }) url!: string;\n\n    public loggedUser: Seller = { slug: '' };\n    public loggedUserToken: any = { token: '' };\n    public link: string = this.url;\n    public expandedMenu: boolean = false;\n    public modalCompartilhe: boolean = false;\n    public modalLinkCompartilhe: boolean = false;\n\n    public isDev = environment.isDevelopment();\n\n    public getUtm = getUtm;\n\n    get isApp() {\n        return this.$store.state.environment.isApp\n    }\n\n    get showBackButton() {\n        return this.isApp && this.$route.name !== 'Dashboard' && this.$route.name !== 'Login' && this.$route.name !== 'Quero Vender';\n    }\n\n    created() {\n        this.getLoggedUser();\n        this.getLoggedUserToken();\n    }\n\n    mounted() {\n        this.getLoggedUser();\n    }\n\n    getLoggedUser() {\n        if (localStorage.getItem('loggedUser')) {\n            this.loggedUser = JSON.parse(localStorage.getItem('loggedUser') || '');\n        }\n    }\n\n    getLoggedUserToken() {\n        if (localStorage.getItem('pertinho')) {\n            this.loggedUserToken = JSON.parse(localStorage.getItem('pertinho') || '{}');\n        }\n    }\n\n    goToBack() {\n        this.$router.back();\n    }\n\n    openMenu() {\n        this.getLoggedUser();\n        this.expandedMenu = !this.expandedMenu;\n    }\n\n    logout() {\n        this.toastMessage('success', 'Até logo ' + this.loggedUser.name);\n        localStorage.clear();\n        this.$router.push('login');\n    }\n\n    private toastMessage(type: SweetAlertType, title: string) {\n        VueSimpleAlert.fire({\n            type,\n            title,\n            toast: true,\n            position: 'top-end',\n            showConfirmButton: false,\n            timer: 5000,\n            onOpen: (toast) => {\n                toast.addEventListener('mouseenter', Swal.stopTimer),\n                toast.addEventListener('mouseleave', Swal.resumeTimer);\n            },\n        });\n    }\n\n    compLink() {\n        (document.getElementById('myTextHeader') as HTMLInputElement).value = `https://www.pertinhodecasa.com.br/vitrine/${this.loggedUser.slug}?${getUtm('link', false)}`;\n    }\n\n    copiarLink() {\n        const copyText = document.getElementById('myTextHeader');\n        (copyText as HTMLInputElement).select();\n        (copyText as HTMLInputElement).setSelectionRange(0, 99999);\n        document.execCommand('copy');\n        alert('Link copiado!');\n    }\n\n    complinkLoja() {\n        window.open(`https://www.pertinhodecasa.com.br/vitrine/${this.loggedUser.slug}?${getUtm('link', false)}`);\n    }\n\n    compWhatsapp() {\n        window.open(`https://api.whatsapp.com/send?text=Ei,%20pessoal!%20Conheçam%20a%20minha%20loja%20dentro%20da%20Pertinho%20de%20Casa!%20Compre%20no%20site%20ou%20pelo%20app!%0Ahttps://www.pertinhodecasa.com.br/vitrine/${this.loggedUser.slug}?${getUtm('whatsapp')}`);\n    }\n\n    compFacebook() {\n        window.open(`https://www.facebook.com/sharer/sharer.php?u=http%3A%2F%2Fwww.pertinhodecasa.com.br%2Fvitrine%2F${this.loggedUser.slug}?${getUtm('facebook')}&src=sdkpreparse`);\n    }\n\n    compMessenger() {\n        window.open(`https://www.facebook.com/login.php?u=http%3A%2F%2Fwww.pertinhodecasa.com.br%2Fvitrine%2F${this.loggedUser.slug}?${getUtm('messenger')}&src=sdkpreparse`);\n    }\n\n    compTwitter() {\n        window.open(`https://twitter.com/intent/tweet?url=https://www.pertinhodecasa.com.br/vitrine/${this.loggedUser.slug}?${getUtm('twitter')}&text=Ei,%20pessoal!%20Conheçam%20a%20minha%20loja%20dentro%20da%20Pertinho%20de%20Casa!%20Compre%20no%20site%20ou%20pelo%20app!`);\n    }\n\n    phoneMask(phone) {\n        return phone.replace(/(\\d{2})(\\d{5})(\\d{4})/, (regex, arg1, arg2, arg3) => `(${arg1}) ${arg2}-${arg3}`);\n    }\n}\n"],"sourceRoot":""}]}