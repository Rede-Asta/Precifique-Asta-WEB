AsCH;AA5ML;;AAAA;AAAA,EAA8B,GAA9B,CAAA;;AAAa,QAAQ,GAAA,UAAA,CAAA,CADpB,SAAS,CAAC,EAAD,CACW,CAAA,EAAR,QAAQ,CAAR;SAAA,Q","sourcesContent":["import {Component, Prop, Vue} from 'vue-property-decorator';\n\nimport productsService from '@/services/products.service';\nimport userService from '@/services/user.service';\nimport categoryService from '@/services/category.service';\nimport {Category} from '@/models/category';\nimport {Product} from '@/models/product';\n\ndeclare global {\n    interface Window {\n        opera: any;\n    }\n}\n\n@Component({})\nexport class Produtos extends Vue {\n    public qntprod: any = '';\n    public moreProduct: boolean = false;\n    public produtos: any[] = [];\n    public allCategories: any[] = [];\n    public allProducts: any[] = [];\n    public loggedUser: any;\n    public showCadastroProduto: boolean = false;\n    public showEditProduct: boolean = false;\n    public showCreateProduct = false;\n\n    private productService = new productsService();\n    private userService = new userService();\n    private categoryService = new categoryService();\n\n    public categorySelected: Category = {\n        name: '',\n        guid: '',\n        deleted: false,\n        description: '',\n        ordering: 0,\n        products: [],\n        vtext_id: 0,\n        vtext_full_path: false,\n        variant_name: '',\n        variant_status: false,\n        variant_value: [\n            {\n                value: '',\n                guid: '',\n            },\n            {\n                value: '',\n                guid: '',\n            },\n        ],\n    };\n\n    public productSelected: Product = {\n        guid: '',\n        name: '',\n        description: '',\n        ordering: 100,\n        is_published: true,\n        skus: [{\n            code: '',\n            guid: '',\n            new_price: '',\n            old_price: '',\n            vtex_id: 0,\n            value: '',\n            in_promotion: false,\n            is_skus: false,\n            status: true,\n        }],\n    };\n\n    public money = {\n        decimal: ',',\n        thousands: '.',\n        prefix: 'R$ ',\n        precision: 2,\n        masked: false,\n    };\n\n    constructor() {\n        super();\n    }\n\n    mounted() {\n        if (localStorage.getItem('loggedUser')) {\n            this.getAllCategoriesProducts();\n        }\n    }\n\n    goToProducts() {\n        this.$router.push('produtos');\n    }\n\n    validateCurrency(x) {\n        let money;\n        if (x) {\n            money = x.replace('R$ ', '');\n            money = money.replace(',', '');\n            money = money.replace('.', '');\n        }\n        return money;\n    }\n\n    public openEditCategoryModal(name: string, guid: string, ordering: number) {\n        const catName = name;\n        const catGuid = guid;\n        const catOrdering = ordering;\n    }\n\n    public openEditProductModal(category: Category, product: Product) {\n        // this.categorySelected = categorySelected;\n        // this.productSelected = productSelected;\n        // this.showEditProduct = true;\n\n        this.$router.push({\n            name: 'ProductEdit',\n            params: {\n                productGuid: product.guid,\n                type: product.register_type === 'Service' ? 'servico' : ''\n            }\n        })\n    }\n\n    public closeProductModal() {\n        this.showEditProduct = false;\n\n        this.destroyProductSelected();\n        this.destroyCategorySelected();\n    }\n\n    private destroyProductSelected() {\n        this.productSelected = {\n            guid: '',\n            name: '',\n            description: '',\n            ordering: 100,\n            is_published: true,\n            skus: [{\n                code: '',\n                guid: '',\n                new_price: '',\n                old_price: '',\n                vtex_id: 0,\n                value: '',\n                in_promotion: false,\n                is_skus: false,\n                status: true,\n            }],\n        };\n    }\n\n    private destroyCategorySelected() {\n        this.categorySelected = {\n            name: '',\n            guid: '',\n            deleted: false,\n            description: '',\n            ordering: 0,\n            products: [],\n            vtext_id: 0,\n            vtext_full_path: false,\n            variant_name: '',\n            variant_status: false,\n            variant_value: [\n                {\n                    value: '',\n                    guid: '',\n                },\n                {\n                    value: '',\n                    guid: '',\n                },\n            ],\n        };\n    }\n\n    getAllCategoriesProducts() {\n        this.qntprod = 0;\n        this.loggedUser = JSON.parse(localStorage.getItem('loggedUser') || '');\n\n        this.categoryService.getCategories(this.loggedUser.slug)\n            .then(data => {\n                if (data.ok) {\n                    data.json().then(async response => {\n                        await response.forEach(category => {\n                            this.productService.getProducts(category.guid)\n                                .then(productData => {\n                                    if (productData.ok) {\n                                        productData.json()\n                                            .then(async (products: any) => {\n                                                await products.map(x => {\n                                                    if (x.skus) {\n                                                        if (parseFloat(this.validateCurrency(x.skus.old_price)) > parseFloat(this.validateCurrency(x.skus.new_price))) {\n                                                            Vue.set(x.skus, 'sale', true);\n                                                            x.skus.sale = true;\n                                                            this.allProducts.push(x);\n                                                        } else {\n                                                            Vue.set(x.skus, 'sale', false);\n                                                            x.skus.sale = false;\n                                                            this.allProducts.push(x);\n                                                        }\n                                                    }\n                                                    this.qntprod = this.qntprod + 1;\n                                                });\n\n                                                const produtos: any[] = [];\n                                                produtos.push(products[0]);\n                                                (produtos[0])\n                                                    ? Vue.set(category, 'products', products)\n                                                    : Vue.set(category, 'products', false);\n                                            });\n                                    }\n                                });\n                        });\n                        this.allCategories = response;\n                    });\n                }\n            });\n    }\n}\n"],"sourceRoot":""}]}