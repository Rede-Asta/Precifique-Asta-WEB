ue {\n\n    private service = new userService();\n\n    public inputs = [\n        {\n            week_day: 1,\n            checkbox: false,\n            label: 'Segunda Feira',\n            input: {\n                opening: '', closing: '',\n            },\n        },\n        {\n            week_day: 2,\n            checkbox: false,\n            label: 'Terça Feira',\n            input: {\n                opening: '', closing: '',\n            },\n        },\n        {\n            week_day: 3,\n            checkbox: false,\n            label: 'Quarta Feira',\n            input: {\n                opening: '', closing: '',\n            },\n        },\n        {\n            week_day: 4,\n            checkbox: false,\n            label: 'Quinta Feira',\n            input: {\n                opening: '', closing: '',\n            },\n        },\n        {\n            week_day: 5,\n            checkbox: false,\n            label: 'Sexta Feira',\n            input: {\n                opening: '', closing: '',\n            },\n        },\n        {\n            week_day: 6,\n            checkbox: false,\n            label: 'Sábado',\n            input: {\n                opening: '', closing: '',\n            },\n        },\n        {\n            week_day: 7,\n            checkbox: false,\n            label: 'Domingo',\n            input: {\n                opening: '', closing: '',\n            },\n        },\n    ];\n\n    private selectAllDays: boolean = false;\n\n    @Watch('inputs', { deep: true })\n    onInputsChange(val) {\n        this.selectAllDays = val.filter(input => !input.checkbox).length === 0;\n    }\n\n\n    constructor() {\n        super();\n        this.service.getHorario()\n        .then(data => data.json())\n        .then(data => {\n            data.forEach(openingHour => {\n                this.inputs.forEach(input => {\n                    if (input.week_day === openingHour.week_day) {\n                        input.checkbox = true;\n                        input.input.opening = openingHour.opening;\n                        input.input.closing = openingHour.closing;\n                    }\n                });\n            });\n        });\n    }\n\n    addZeros() {\n        let i = 1;\n        this.inputs.forEach((data) => {\n            while (data.input.closing.length < 5 && data.input.closing.length > 0) {\n                if (data.input.closing.length < 2) {\n                    data.input.closing = '0' + data.input.closing;\n                    i = 2;\n                }\n                if (data.input.closing.length > 3 && i === 1) {\n                    data.input.closing = '0' + data.input.closing;\n                    i = 2;\n                } else {\n                    data.input.closing += '0';\n                    i = 2;\n                }\n            }\n            while (data.input.opening.length < 5 && data.input.opening.length > 0) {\n                if (data.input.opening.length < 2) {\n                    data.input.opening = '0' + data.input.opening;\n                    i = 2;\n                }\n                if (data.input.opening.length > 3 && i === 1) {\n                    data.input.opening = '0' + data.input.opening;\n                    i = 2;\n                } else {\n                    data.input.opening += '0';\n                    i = 2;\n                }\n            }\n        });\n    }\n\n    async salvar() {\n        const horarios: object[] = [];\n        this.inputs.forEach((data) => {\n                if (data.checkbox) {\n                    horarios.push({ week_day: data.week_day, opening: data.input.opening, closing: data.input.closing });\n                }\n            });\n        this.service.updateHorario(horarios)\n        .then(data => {\n            if (data.ok) {\n                // this.$router.push('dashboard')\n                this.$emit('notification', {\n                    icon: require('./../../../assets/images/icon_chat.png'),\n                    text: `Alterações salvas com sucesso`,\n                    color: '#c3e4e7',\n                    active: true,\n                    type: 'error',\n                });\n            } else {\n                this.$emit('notification', {\n                    icon: require('./../../../assets/images/icon_error.png'),\n                    text: 'Por favor preencha o horário de atendimento',\n                    color: '#ffd1c4',\n                    active: true,\n                    type: 'error',\n                });\n            }\n        });\n    }\n\n    checkChanged(event) {\n        this.inputs.forEach(data => {\n            if (data.week_day === event.target.id) {\n                data.checkbox = event.target.checked;\n            }\n        });\n    }\n\n    onSelectAllDaysChange(event) {\n        this.inputs.forEach(input => input.checkbox = event.target.checked);\n    }\n}\n"],"sourceRoot":""}]}