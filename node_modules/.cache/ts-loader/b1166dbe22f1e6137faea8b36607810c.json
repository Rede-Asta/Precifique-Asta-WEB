oduct(product: Product) {\n    this.productService\n      .deleteProduct(product.guid)\n      .then(() =>\n        this.nextStatusSuccess(\n          `Pronto, o Produto ${product.name} já foi excluída da sua vitrine.`\n        )\n      )\n      .catch(() =>\n        this.nextStatusError(\n          `Desculpe, ocorreu algum erro ao excluír o Produto ${product.name}, por favor tente novamente mais tarde`\n        )\n      );\n  }\n\n  private nextStatusSuccess(msg: string) {\n    this.$emit(\"loading\", false);\n    this.$emit(\"getCategories\", true);\n    this.$emit(\"closeAllModal\", true);\n    this.destroySelected();\n    this.closeDeleteProductModal();\n    this.toastMessage(\"success\", msg);\n  }\n\n  private nextStatusError(msg: string) {\n    this.$emit(\"loading\", false);\n    this.closeDeleteProductModal();\n    this.toastMessage(\"error\", msg);\n  }\n\n  public closeDeleteProductModal() {\n    this.$emit(\"closeDeleteProductModal\", true);\n  }\n\n  public destroySelected() {\n    this.$emit(\"destroyProductSelected\", true);\n    this.$emit(\"destroyCategorySelected\", true);\n  }\n\n  private toastMessage(type: SweetAlertType, title: string) {\n    VueSimpleAlert.fire({\n      type,\n      title,\n      toast: true,\n      position: \"top-end\",\n      showConfirmButton: false,\n      timer: 3000,\n      onOpen: (toast) => {\n        toast.addEventListener(\"mouseenter\", Swal.stopTimer),\n          toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n      },\n    });\n  }\n}\n\nexport { ProductDelete as default };\n"],"sourceRoot":""}]}