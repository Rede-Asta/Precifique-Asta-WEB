 \"vue-simple-alert\";\nimport Swal, { SweetAlertType } from \"sweetalert2\";\nimport moment from \"moment\";\n\nimport OrderService from \"@/services/orders.service\";\nimport { Order } from \"@/models/order\";\n\n@Component({\n  directives: { money: VMoney },\n})\nexport class Tracking extends Vue {\n  private orderService = new OrderService();\n\n  @Prop({ required: true }) public propOrder!: Order;\n  @Prop({ required: true }) public propOrderNumber!: string;\n  @Prop({ required: true }) public propVerification!: boolean;\n  @Prop({ required: true }) public propLoading!: boolean;\n\n  @Prop({ default: true, required: false }) public propTitle!: boolean;\n\n  @Prop() propPrevDisabled!: boolean;\n\n  public order: Order = this.propOrder;\n  public orderNumber: string = this.propOrderNumber;\n  public verification: boolean = this.propVerification;\n  public loading: boolean = this.propLoading;\n  public viewTitle: boolean = this.propTitle;\n  public title = \"Acompanhamento do Pedido\";\n  public money = {\n    decimal: \",\",\n    thousands: \".\",\n    prefix: \"R$ \",\n    suffix: \"\",\n    precision: 2,\n    masked: false,\n  };\n  public moneyCash = {\n    decimal: \",\",\n    thousands: \".\",\n    prefix: \"R$ \",\n    suffix: \" à vista\",\n    precision: 2,\n    masked: false,\n  };\n  public statusItems = [\n    { name: \"Pedido Efetuado\", status: \"CREATED\" },\n    { name: \"Pedido recebido pelo vendedor\", status: \"RECEIVED SELLER\" },\n    { name: \"Pedido saiu para entrega\", status: \"READY DELIVERY\" },\n    { name: \"Pedido entregue\", status: \"DELIVERED\" },\n  ];\n\n  constructor() {\n    super();\n    document.title = this.title;\n  }\n\n  private async updateStatus(statusNumber: number) {\n    await this.orderService\n      .updateOrder(this.orderNumber, statusNumber)\n      .then((data) => {\n        if (data.ok) {\n          VueSimpleAlert.fire({\n            title: this.setNewStatus(this.order.order_status?.label),\n            text: \"Status do Pedido Atualizado!\",\n            type: \"success\",\n            timer: 3000,\n          }).then(() =>\n            data\n              .json()\n              .then((order) => (this.order.order_status = order.order_status))\n          );\n        } else {\n          VueSimpleAlert.fire({\n            title: \"Erro ao alterar Status do Pedido\",\n            text: \"Não é possível alterar o status o pedido.\",\n            type: \"error\",\n            timer: 4000,\n          }).then(() => this.$router.push(\"dashboard\"));\n        }\n      })\n      .catch((err) => console.error(err));\n  }\n\n  public checkUpdateStatus(value: number) {\n    VueSimpleAlert.fire({\n      title: `${this.setStatus(value).text}?`,\n      text: `Tem certeza que deseja ${this.setStatus(value).text}?`,\n      type: \"warning\",\n      showCancelButton: true,\n      cancelButtonText: \"Não\",\n      confirmButtonText: \"Sim\",\n      customClass: {\n        title: \"title-modal\",\n        content: \"text-uppercase\",\n      },\n    }).then((result) => {\n      if (result.value) {\n        this.updateStatus(value + 1);\n      }\n    });\n  }\n\n  public goPrint() {\n    window.print();\n  }\n\n  public checkCanceledStatus(guid: string) {\n    VueSimpleAlert.fire({\n      title: `Cancelar Pedido?`,\n      text: `Tem certeza que deseja cancelar o pedido?`,\n      type: \"warning\",\n      showCancelButton: true,\n      cancelButtonText: \"Não\",\n      confirmButtonText: \"Sim\",\n      customClass: {\n        title: \"title-modal\",\n        content: \"text-uppercase\",\n      },\n    }).then((result) => {\n      if (result.value) {\n        this.cancelOrder(guid);\n      }\n    });\n  }\n\n  public cancelOrder = (orderGuid: string) =>\n    this.orderService\n      .cancelOrder(orderGuid)\n      .then(() => {\n        this.$emit(\"getOrder\", true);\n        if (this.order.status) {\n          this.order.status[1] = \"CANCELED\";\n        }\n      })\n      .catch(() =>\n        this.setToastAlert(\n          \"error\",\n          `No momento não foi possivel cancelar esse pedido.`,\n          4000\n        )\n      );\n\n  setToastAlert(type: SweetAlertType, title: string, timer: number) {\n    VueSimpleAlert.fire({\n      type,\n      title,\n      toast: true,\n      position: \"top-end\",\n      showConfirmButton: false,\n      timer,\n      onOpen: (toast) => {\n        toast.addEventListener(\"mouseenter\", Swal.stopTimer),\n          toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n      },\n    });\n  }\n\n  public generateLink() {\n    VueSimpleAlert.fire({\n      html: `\n                <div class=\"col-12 mb-3\">\n                    <h4 class=\"m-0 font-weight-bold\">Copie o link e envie ao responsável pela entrega</h4>\n                </div>\n                <div class=\"input-group mb-3\">\n                    <input type=\"text\" id=\"swal-input1\" class=\"form-control\" readonly>\n                    <div class=\"input-group-append\">\n                        <button id=\"copy\" class=\"btn btn-outline-secondary\" type=\"button\">\n                            Copiar\n                        </button>\n                    </div>\n                </div>\n            `,\n      inputValue: \"\",\n      onOpen: () => {\n        const pertinho = JSON.parse(localStorage.getItem(\"pertinho\") || \"{}\");\n        const link = `https://www.pertinhodecasa.com.br/meus-pedidos/${pertinho.guid}/${this.orderNumber}`;\n        const content = Swal.getContent();\n        const input1 = content.querySelector(\n          \"#swal-input1\"\n        ) as HTMLInputElement;\n        const confirm = content.querySelector(\"#copy\") as HTMLInputElement;\n        if (input1) {\n          input1.value = link;\n          confirm.addEventListener(\"click\", (e) => {\n            if (e) {\n              const copyText = document.getElementById(\n                \"swal-input1\"\n              ) as HTMLInputElement;\n              copyText.select();\n              copyText.setSelectionRange(0, 99999);\n              document.execCommand(\"copy\");\n              VueSimpleAlert.fire({\n                type: \"success\",\n                title: \"Link copiado para área de transferência\",\n                toast: true,\n                position: \"top-end\",\n                showConfirmButton: false,\n                timer: 3000,\n                onOpen: (toast) => {\n                  toast.addEventListener(\"mouseenter\", Swal.stopTimer),\n                    toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n                },\n              });\n            }\n          });\n        }\n      },\n      showCancelButton: false,\n      showConfirmButton: false,\n    });\n  }\n\n  public formatDate(data: Date) {\n    if (data) {\n      return moment(String(data)).utc().locale(\"pt-br\").format(\"LL\");\n    }\n  }\n\n  public formatEstimatedTime(time: string) {\n    if (time) {\n      switch (time) {\n        case \"00:00:00\":\n          return \"à combinar\";\n        case \"01:00:00\":\n          return \"1 hora\";\n        case \"23:59:59\":\n        case \"1d\":\n          return \"1 dia\";\n      }\n    }\n  }\n\n  public formatCountry(country: string) {\n    if (country) {\n      switch (country) {\n        case \"BR\":\n          return \"Brasil\";\n        case \"US\":\n          return \"Estados Unidos\";\n        default:\n          return country;\n      }\n    }\n  }\n\n  public setClass = (myClass: string): string =>\n    myClass.toLowerCase().replace(\" \", \"_\");\n\n  public setNewStatus(status: string | undefined) {\n    switch (status) {\n      case \"CREATED\":\n        return \"Pedido recebido pelo vendedor\";\n      case \"RECEIVED SELLER\":\n        return \"Pedido saiu para entrega\";\n      case \"READY DELIVERY\":\n        return \"Pedido entregue\";\n      case \"DELIVERED\":\n        return \"Pedido será finalizado\";\n    }\n  }\n\n  public setStatus(status: number) {\n    const nameStatus = { text: \"\", icon: \"\" };\n    switch (status) {\n      case 1:\n        nameStatus.text = \"Confirmar recebimento do pedido\";\n        nameStatus.icon = \"local_mall\";\n        break;\n      case 2:\n        nameStatus.text = \"Avisar que o pedido saiu para entrega\";\n        nameStatus.icon = \"local_shipping\";\n        break;\n      case 3:\n        nameStatus.text = \"Confirmar entrega do pedido\";\n        nameStatus.icon = \"cottage\";\n        break;\n      default:\n        break;\n    }\n    return nameStatus;\n  }\n}\n\nexport { Tracking as default };\n"],"sourceRoot":""}]}