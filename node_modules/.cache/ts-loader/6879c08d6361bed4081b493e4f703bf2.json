GoB,MArGpB,EAqG8C;AACtC,cAAQ,MAAR;AACI,aAAK,SAAL;AACI,iBAAO,iBAAP;;AACJ,aAAK,iBAAL;AACI,iBAAO,+BAAP;;AACJ,aAAK,gBAAL;AACI,iBAAO,0BAAP;;AACJ,aAAK,WAAL;AACI,iBAAO,iBAAP;AARR;AAUH;AAhHL;AAAA;AAAA,6BAkHa,OAlHb,EAkH4B;AACpB,aAAO,OAAO,CAAC,WAAR,GAAsB,OAAtB,CAA8B,GAA9B,EAAmC,GAAnC,CAAP;AACH;AApHL;AAAA;AAAA,uCAsHoB;AACZ,WAAK,KAAL,CAAW,YAAX,EAAyB,IAAzB;AACH;AAxHL;AAAA;AAAA,oCA0HiB;AACT,WAAK,UAAL,GAAkB,QAAlB;AACA,WAAK,kBAAL,GAA0B,KAA1B;AAEA,MAAA,UAAU,CAAC,UAAC,KAAD,EAAU;AACjB,QAAA,KAAK,CAAC,eAAN,GAAwB,IAAxB;AACH,OAFS,EAEP,GAFO,EAEF,IAFE,CAAV;AAGH;AAjIL;AAAA;AAAA,kCAmIkB,KAnIlB,EAmI8B;AACtB,WAAK,UAAL,GAAkB,QAAlB;AACA,WAAK,eAAL,GAAuB,KAAvB;AACA,WAAK,KAAL,GAAa,KAAb;AAEA,MAAA,UAAU,CAAC,UAAC,KAAD,EAAU;AACjB,QAAA,KAAK,CAAC,kBAAN,GAA2B,IAA3B;AACH,OAFS,EAEP,GAFO,EAEF,IAFE,CAAV;AAGH;AA3IL;;AAAA;AAAA,EAA+B,GAA/B,CAAA;;AAI4B,UAAA,CAAA,CAAvB,IAAI,CAAC;AAAC,EAAA,QAAQ,EAAE;AAAX,CAAD,CAAmB,CAAA,E,mBAAA,E,WAAA,E,KAAmB,CAAnB,CAAA;;AACC,UAAA,CAAA,CAAxB,IAAI,CAAC;AAAC,EAAA,QAAQ,EAAE;AAAX,CAAD,CAAoB,CAAA,E,mBAAA,E,WAAA,E,KAAkB,CAAlB,CAAA;;AALhB,SAAS,GAAA,UAAA,CAAA,CALrB,SAAS,CAAC;AACP,EAAA,UAAU,EAAE;AACR,IAAA,QAAQ,EAAE;AADF;AADL,CAAD,CAKY,CAAA,EAAT,SAAS,CAAT;SAAA,S","sourcesContent":["import moment from 'moment';\nimport { Component, Prop, Vue } from 'vue-property-decorator';\nimport Paginate                 from 'vuejs-paginate';\nimport OrderService from '@/services/orders.service';\n\nimport { IOrder, Order } from '@/models/order';\n\n\n@Component({\n    components: {\n        paginate: Paginate,\n    },\n})\nexport class orderList extends Vue {\n\n    private orderService = new OrderService();\n\n    @Prop({required: true}) propTitle!: string;\n    @Prop({required: false}) propOrder!: Order;\n\n    public title: string = this.propTitle;\n    public subTitle: string = 'Acompanhe todos seus pedidos e status!';\n\n    public titleTracking = 'Acompanhamento do Pedido';\n\n    public orderList: Order[][] = [];\n    public orderListMock = [1, 2, 3, 4, 5];\n\n    public order: Order = { code: 0 };\n    public orderNumber: string = '';\n    public transitionOrder: boolean = true;\n    public transitionTracking: boolean = false;\n    public transition: string = 'in-out';\n\n    public historyTotal: number = -1;\n    public createdTotal: number = 0;\n    public receivedTotal: number = 0;\n    public readyDeliveryTotal: number = 0;\n    public deliveredTotal: number = 0;\n    public historyTab: string = '1';\n\n    public index: number = 1;\n    public pageCurrent: number = 1;\n    public pageCount: number = 1;\n\n    constructor() {\n        super();\n    }\n\n    mounted() {\n        this.getPaginateOrdersList(1, 1);\n\n        if (this.propOrder.code !== 0) {\n            this.openOrderItem(this.propOrder);\n        }\n    }\n\n    getOrdersList() {\n        this.orderService.getOrdersList()\n            .subscribe((orders: Order[]) => {\n                this.orderList = this.groupBy(orders, 'order_status');\n                this.historyTotal = orders.length;\n                this.setLengthGroups(this.orderList);\n                this.index++;\n        });\n    }\n\n    public getPaginateOrdersList(page: number, orderStatus: number) {\n        this.historyTotal = -1;\n        this.orderService.getPaginateOrdersList(page, orderStatus)\n            .subscribe((orders: IOrder) => {\n                this.orderList[orderStatus] = orders.data;\n                this.historyTotal = orders.data.length;\n                this.pageCount = orders.number_pages_status;\n                this.setLengthGroups(orders.list_status);\n                this.index++;\n        });\n    }\n\n    public changeSearchPage(page: number) {\n        this.getPaginateOrdersList(page, +this.historyTab);\n    }\n\n    groupBy(array: any[], key: string | number) {\n        return array.reduce((objectsByKeyValue, obj) => ({ ...objectsByKeyValue, [obj[key].value]: (objectsByKeyValue[obj[key].value] || []).concat(obj) }), {});\n    }\n\n    setLengthGroups(orders) {\n        this.createdTotal = orders[0].created;\n        this.receivedTotal = orders[1].received_seller;\n        this.readyDeliveryTotal = orders[2].ready_delivery;\n        this.deliveredTotal = orders[3].delivered;\n\n        if (!this.createdTotal && !this.receivedTotal && !this.readyDeliveryTotal && this.deliveredTotal) {\n            this.setHistoryTab('4');\n        } else if (!this.createdTotal && !this.receivedTotal && this.readyDeliveryTotal) {\n            this.setHistoryTab('3');\n        } else if (!this.createdTotal && this.receivedTotal) {\n            this.setHistoryTab('2');\n        }\n    }\n\n    setHistoryTab(indexTab: string) {\n        if (indexTab !== this.historyTab) {\n            this.historyTab = indexTab;\n            this.pageCurrent = 1;\n            this.getPaginateOrdersList(this.pageCurrent, +indexTab);\n        }\n    }\n\n    formatDate(data) {\n        return moment(String(data)).utc().locale('pt-br').format('DD/MM/YYYY à HH:mm').replace('à', 'às').replace(':', 'h');\n    }\n\n    translateStatus(status: string | undefined) {\n        switch (status) {\n            case 'CREATED':\n                return 'Pedido Efetuado';\n            case 'RECEIVED SELLER':\n                return 'Pedido recebido pelo vendedor';\n            case 'READY DELIVERY':\n                return 'Pedido saiu para entrega';\n            case 'DELIVERED':\n                return 'Pedido entregue';\n        }\n    }\n\n    setClass(myClass: string): string {\n        return myClass.toLowerCase().replace(' ', '_');\n    }\n\n    closeModalOrders() {\n        this.$emit('closeModal', true);\n    }\n\n    closeTracking() {\n        this.transition = 'in-out';\n        this.transitionTracking = false;\n\n        setTimeout((scope) => {\n            scope.transitionOrder = true;\n        }, 500, this);\n    }\n\n    openOrderItem(order: Order) {\n        this.transition = 'out-in';\n        this.transitionOrder = false;\n        this.order = order;\n\n        setTimeout((scope) => {\n            scope.transitionTracking = true;\n        }, 500, this);\n    }\n}\n"],"sourceRoot":""}]}