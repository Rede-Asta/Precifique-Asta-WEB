AAZ,CAAkB,YAAlB,IAAkC,KAAK,MAAL,CAAY,KAAZ,CAAkB,YAAlB,CAA+B,SAAxE;AACH;AApBL;AAAA;AAAA,wBAsB2B;AACnB,aAAO,KAAK,MAAL,CAAY,KAAZ,CAAkB,YAAlB,IAAkC,KAAK,MAAL,CAAY,KAAZ,CAAkB,YAAlB,CAA+B,OAAxE;AAIH;AA3BL;;AAAA;AAAA,EAAmC,GAAnC,CAAA;;AAEkC,UAAA,CAAA,CAA7B,IAAI,CAAC;AAAE,EAAA,OAAO,EAAE;AAAX,CAAD,CAAyB,CAAA,E,uBAAA,E,QAAA,E,KAAqC,CAArC,CAAA;;AACb,UAAA,CAAA,CAAhB,KAAK,CAAC,QAAD,CAAW,CAAA,E,uBAAA,E,MAAA,EAEhB,IAFgB,CAAA;;AAHR,aAAa,GAAA,UAAA,CAAA,CARzB,SAAS,CAAC;AACP,EAAA,UAAU,EAAE;AACR,IAAA,QAAQ,EAAR,QADQ;AAER,IAAA,KAAK,EAAL,KAFQ;AAGR,IAAA,sBAAsB,EAAtB;AAHQ;AADL,CAAD,CAQgB,CAAA,EAAb,aAAa,CAAb;SAAA,a","sourcesContent":["import { Component, Prop, Vue, Watch } from 'vue-property-decorator';\nimport { Carousel, Slide } from 'vue-carousel';\nimport Swal, { SweetAlertType } from 'sweetalert2';\nimport VueSimpleAlert from 'vue-simple-alert';\n\nimport INotification from '@/models/INotification';\nimport Seller from '@/models/seller';\nimport productsService from '@/services/products.service';\nimport { listStatusSellerValid } from '@/environments/list_status_valid';\nimport userService from '@/services/user.service';\nimport CoursesBannersCarousel from \"@/components/courses/CoursesBannersCarousel.vue\";\n\n@Component({\n    components: {\n        Carousel,\n        Slide,\n        CoursesBannersCarousel\n    },\n})\n\nexport class dashboardView extends Vue {\n\n    @Prop({ default: undefined }) notify?: INotification | string = ``;\n    @Watch('notify') show(value: string, oldValue: string) {\n        console.log(value, oldValue);\n    }\n\n    public showNewFunctions: boolean = false;\n    public textModal: string = '';\n    public loggedUser: Seller = { slug: '' };\n\n    private productsService = new productsService();\n    private userService = new userService();\n\n    constructor() {\n        super();\n    }\n\n    get featureFlags() {\n        return this.$store.state.featureFlags && this.$store.state.featureFlags.dashboard;\n    }\n\n    get featureFlagsLanding() {\n        return this.$store.state.featureFlags && this.$store.state.featureFlags.landing;\n\n\n\n    }\n\n    mounted() {\n        this.getUser();\n\n        if (localStorage.getItem('notificacao')) {\n            this.showNewFunctions = false;\n        } else {\n            this.setTextModal().then(r => {\n                localStorage.setItem('notificacao', 'true');\n            });\n        }\n    }\n\n    private async setTextModal() {\n        const response = await this.userService.getModalAdmin();\n        this.textModal = response['message'];\n        this.showNewFunctions = response['visible'];\n    }\n\n    private async getUser() {\n        const response = await this.productsService.getProdcutsAll();\n        switch (response.status) {\n            case 200:\n            case 201:\n            case 202:\n                const data = await response.json();\n                if (!data.seller_disable) {\n                    localStorage.setItem('loggedUser', JSON.stringify(data));\n                    this.loggedUser = JSON.parse(localStorage.getItem('loggedUser') || '');\n                    // this.toastMessage('success', `Bem vindo, de volta ${loggedUser.name}!`);\n                    if (!listStatusSellerValid.includes(data.account_status)) {\n                        this.redirectPageRegister(data.account_status);\n                    }\n                    break;\n                }else{  \n                    this.notificar({\n                        icon: require('./../../assets/images/icon_error.png'),\n                        text: `\n                            <strong>Seu estabelecimento está desativado!</strong>\n                            <br/>\n                            Clique no botão abaixo para reativar sua vitrine na Pertinho de Casa\n                        `,\n                        color: '#ffd1c4',\n                        active: true,\n                        type: 'error',\n                        // url: false,\n                        text_align_left: true,\n                        buttons: true,\n                        buttonText: 'Reativar conta',\n                        activeButtons: true,\n                        activeClose: data.seller_disable,\n                        buttonAction: true\n                    });\n                } \n               \n            case 400:\n            case 404:\n            case 423:\n                const error = await response.json();\n                this.toastMessage('error', error.detail ? error.detail : error);\n                break;\n        }\n    }\n\n    private redirectPageRegister(accountStatus: string) {\n        this.$router.push(`quero-vender?etapa=${accountStatus}`);\n    }\n\n    private toastMessage(type: SweetAlertType, title: string) {\n        VueSimpleAlert.fire({\n            type,\n            title,\n            toast: true,\n            position: 'top-end',\n            showConfirmButton: false,\n            timer: 5000,\n            onOpen: (toast) => {\n                toast.addEventListener('mouseenter', Swal.stopTimer),\n                    toast.addEventListener('mouseleave', Swal.resumeTimer);\n            },\n        });\n    }\n\n    public closeModal() {\n        this.showNewFunctions = false;\n        localStorage.setItem('notificacao', 'false');\n    }\n\n    public notificar(data) {\n        this.notify = data;\n    }\n}\n"],"sourceRoot":""}]}