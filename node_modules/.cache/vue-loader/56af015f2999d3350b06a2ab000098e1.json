class=\"button-enter-code-precifique col-10 col-xl-5\" @click=\"submitForm()\">Enviar</button>\n                </div>\n</form>\n  </section>\n\n\n</template>\n\n<script lang=\"ts\">\n\nimport { InputForms, TypeHelp } from \"@/models/inputForms\";\n  import RegisterSellerService from \"@/services/register.service\";\n  import { Component, Prop, Vue } from \"vue-property-decorator\";\n  import PrecifiqueService from \"@/services/precifiqueService\";\n\n\n  @Component({})\n  export class CodePrecifiqueForm extends Vue {\n    @Prop() propArrayInputs!: InputForms[];\n    @Prop() propEmail!: string;\n\n    public arrayInputs: InputForms[] = this.propArrayInputs;\n    public emailButton: boolean = false;\n     \n  public email: string = this.propEmail;\n  public isValidCode: boolean = true\n  public code: string = '';\n\n  \n                validateCode() {\n                    // Implemente a validação de email aqui\n                    // Por exemplo, use uma expressão regular para validar o formato do email\n                    const codeRegex = /^\\d{4}$/;\n                    this.isValidCode = codeRegex.test(this.code);\n                }\n\n                public async submitForm() {\n                    if (this.isValidCode) {\n                      \n    // setTimeout(() => {this.sendEmail()}, 10000);\n\n    const precifiqueService = new PrecifiqueService();\n\n\n     try {\n \n    \n\n       await precifiqueService.sendEmail({\n            email: this.email,\n            \n         });\n\n        this.$alert('Email enviado com sucesso', '', 'success', {\n             toast: true,\n             position: 'top-end',\n             showConfirmButton: false,\n             timer: 3000,\n\n             \n         })\n\n        \n         ;\n     } catch (e) {\n         console.error(e);\n         this.$alert('Erro ao enviar o email', '', 'error', {\n             toast: true,\n             position: 'top-end',\n             showConfirmButton: false,\n             timer: 3000,\n         })\n\n         \n         ;\n     }\n\n\n\n\n\n    \n}\n                    } \n                }\n      \n                \n\n\n  \n\n  export { CodePrecifiqueForm as default };\n\n\n</script>\n\n\n<style lang=\"scss\" scoped>\n.form-precifique{\n  display: block;\n    width: 100%;\n    height: calc(1.5em + 0.75rem + 2px);\n    padding: 0.375rem 0.75rem;\n    font-size: 1rem;\n    font-weight: 400;\n    line-height: 1.5;\n    color: #495057;\n    background-color: #fff;\n    background-clip: padding-box;\n    border: 1px solid #ced4da;\n    border-radius: 0.25rem;\n    transition: border-color 0.15s ease-in-out, box-shadow 0.15s\n}\n\n.error{\n  color: $error;\n}\n\ninput:focus {\n            border-color: $primary; /* Cor azul de foco (pode personalizar a cor) */\n            outline: none; /* Remova a borda de contorno padrão que alguns navegadores adicionam */\n        }\n\n\n</style>"]}]}