percase\">\n          {{ title }}\n        </h4>\n        <p class=\"text-muted text-center mb-1\">{{ subtitle }}</p>\n        <small class=\"text-muted\">{{ thirdTitle }}</small>\n\n        <div class=\"d-flex justify-content-between mt-4\">\n          <button class=\"btn btn-light\" @click=\"cancelPublishedModal()\">\n            <i class=\"fas fa-times mr-2\"></i>\n            Cancelar\n          </button>\n          <button class=\"btn btn-warning\" @click=\"publishedProduct()\">\n            <i\n              class=\"fas mr-2\"\n              :class=\"{\n                'fa-store-slash': productSelected.is_published,\n                'fa-store': !productSelected.is_published,\n              }\"\n            ></i>\n            {{ titleBtn }}\n          </button>\n        </div>\n      </div>\n    </div>\n  </modal>\n</template>\n\n<script lang=\"ts\">\n  import { Component, Prop, Vue } from \"vue-property-decorator\";\n  import { Product } from \"@/models/product\";\n  \n  @Component({})\n  export class ProductPublished extends Vue {\n    @Prop() propProductSelected!: Product;\n    @Prop() propShowPublishedProduct!: boolean;\n    @Prop() propRegisterType!: string;\n\n    public productSelected: Product = this.propProductSelected;\n    public showPublishedProduct: boolean = this.propShowPublishedProduct;\n    public registerType: string = this.propRegisterType;\n\n    public title: string = \"\";\n    public subtitle: string = \"\";\n    public titleBtn: string = \"\";\n    public thirdTitle: string = \"\";\n\n    created() {\n      if (this.productSelected.is_published) {\n        if (this.registerType === \"Product\") {\n          this.setTitles(\n            \"Pausar Anúncio do Produto\",\n            \"Tem certeza que deseja Pausar o anúncio do produto?\",\n            `Pausando o anúncio deste produto ${this.productSelected.name}, ele não estará disponível aos compradores.`,\n            \"Pausar Anúncio\"\n          );\n        } else if (this.registerType === \"Service\") {\n          this.setTitles(\n            \"Pausar Anúncio do Serviço\",\n            \"Tem certeza que deseja Pausar o anúncio do serviço?\",\n            `Pausando o anúncio deste serviço ${this.productSelected.name}, ele não estará disponível aos compradores.`,\n            \"Pausar Anúncio\"\n          );\n        }\n      } else {\n        if (this.registerType === \"Product\") {\n          this.setTitles(\n            \"Retomar Anúncio do Produto\",\n            \"Tem certeza que deseja Retomar o anúncio do produto?\",\n            `Despausando o anúncio deste produto ${this.productSelected.name}, ele estará disponível aos compradores.`,\n            \"Retomar Anúncio\"\n          );\n        } else if (this.registerType === \"Service\") {\n          this.setTitles(\n            \"Retomar Anúncio do Serviço\",\n            \"Tem certeza que deseja Retomar o anúncio do serviço?\",\n            `Despausando o anúncio deste serviço ${this.productSelected.name}, ele estará disponível aos compradores.`,\n            \"Retomar Anúncio\"\n          );\n        }\n      }\n    }\n\n    private setTitles(\n      title: string,\n      subtitle: string,\n      thirdTitle: string,\n      titleBtn: string\n    ) {\n      this.title = title;\n      this.subtitle = subtitle;\n      this.thirdTitle = thirdTitle;\n      this.titleBtn = titleBtn;\n    }\n\n    public closePublishedModal() {\n      this.$emit(\"closePublishedModal\", true);\n      this.cancelPublishedModal();\n    }\n\n    public cancelPublishedModal() {\n      this.$emit(\"cancelPublishedModal\", true);\n    }\n\n    public publishedProduct() {\n      this.$emit(\"publishedProduct\", true);\n    }\n  }\n\n  export { ProductPublished as default };\n</script>\n\n<style lang=\"scss\" scoped>\n  .custom-modal {\n    width: 25vw;\n\n    .btn {\n      font-size: 14px;\n    }\n\n    .slider {\n      top: 6px !important;\n    }\n\n    .variant {\n      font-size: 14px;\n    }\n\n    @media screen and (max-width: 992px) {\n      width: 98vw;\n      height: auto;\n      border-radius: 6px;\n\n      .btn {\n        font-size: 12px;\n      }\n\n      h4 {\n        margin-top: 20px;\n      }\n    }\n\n    @media screen and (min-width: 992px) and (max-width: 1360px) {\n      width: 40vw;\n    }\n  }\n</style>"]}]}