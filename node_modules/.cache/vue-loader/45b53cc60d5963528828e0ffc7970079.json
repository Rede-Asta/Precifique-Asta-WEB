{"remainingRequest":"/home/aline/GitHub/admin-frontend/node_modules/vue-loader/lib/index.js??vue-loader-options!/home/aline/GitHub/admin-frontend/src/components/orders/tracking/Tracking.vue?vue&type=script&lang=ts&","dependencies":[{"path":"/home/aline/GitHub/admin-frontend/src/components/orders/tracking/Tracking.vue","mtime":1673321154838},{"path":"/home/aline/GitHub/admin-frontend/node_modules/cache-loader/dist/cjs.js","mtime":1673370377252},{"path":"/home/aline/GitHub/admin-frontend/node_modules/babel-loader/lib/index.js","mtime":1673370377198},{"path":"/home/aline/GitHub/admin-frontend/node_modules/ts-loader/index.js","mtime":1673370387800},{"path":"/home/aline/GitHub/admin-frontend/node_modules/cache-loader/dist/cjs.js","mtime":1673370377252},{"path":"/home/aline/GitHub/admin-frontend/node_modules/vue-loader/lib/index.js","mtime":1673370389594}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Tracking.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAscA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA","file":"Tracking.vue","sourceRoot":"src/components/orders/tracking","sourcesContent":["<template>\n  <section>\n    <loading :active=\"loading\"></loading>\n\n    <section class=\"container\" v-if=\"order && verification\">\n      <div class=\"row hidden-print\" v-if=\"viewTitle\">\n        <h1 class=\"h4 mb-4 col-12 p-1\">{{ title + \" - #\" + order.code }}</h1>\n      </div>\n\n      <section class=\"row\" v-if=\"order.order_status\">\n        <div class=\"col-md-12 col-lg-12 col-xl-5 col-sm-12 p-1\">\n          <div class=\"card shadow-lg background-blue card-order\">\n            <div class=\"card-body\">\n              <h5 class=\"card-title text-white print-principal-info\">\n                Pedido nº\n              </h5>\n              <p class=\"card-title font-weight-bold text-white\">\n                {{ \"#\" + order.code }}\n              </p>\n              <hr class=\"divisor\" />\n              <h5 class=\"card-title text-white\">Data do pedido:</h5>\n              <p class=\"card-text font-weight-bold text-white\">\n                {{ formatDate(order.modified) }}\n              </p>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"col-md-12 col-lg-12 col-xl-7 col-sm-12 p-1 hidden-print\">\n          <div class=\"card shadow-lg\">\n            <div class=\"card-body\">\n              <div class=\"row p-0\">\n                <div class=\"col-md-6 col-sm-12 d-flex p-0\">\n                  <div\n                    class=\"timeline d-flex\"\n                    :class=\"setClass(order.order_status.label)\"\n                  >\n                    <div\n                      class=\"timeline-bar\"\n                      :class=\"{\n                        'timeline-bar-actived': order.status[1] !== 'CANCELED',\n                        'timeline-bar-canceled': order.status[1] === 'CANCELED',\n                      }\"\n                    ></div>\n                  </div>\n                  <ul\n                    class=\"d-flex flex-column justify-content-center m-0\"\n                    :class=\"setClass(order.order_status.label)\"\n                  >\n                    <li v-for=\"status in statusItems\">\n                      <span\n                        class=\"dots\"\n                        :class=\"{\n                          'dots-actived': order.status[1] !== 'CANCELED',\n                          'dots-canceled': order.status[1] === 'CANCELED',\n                        }\"\n                        :title=\"status.name\"\n                      ></span>\n                      {{ status.name }}\n                    </li>\n                  </ul>\n                </div>\n                <div\n                  class=\"\n                    col-md-6 col-sm-12\n                    p-0\n                    mx-auto\n                    mt-2\n                    d-flex\n                    justify-content-center\n                    align-items-center\n                    flex-column\n                    m-auto\n                  \"\n                >\n                  <template\n                    v-if=\"\n                      order.order_status.value >= 1 &&\n                      order.order_status.value < 4 &&\n                      order.status[1] !== 'CANCELED'\n                    \"\n                  >\n                    <button\n                      class=\"\n                        col-12\n                        btn btn-primary btn-status-order\n                        d-flex\n                        justify-content-center\n                        align-items-center\n                        pulse\n                      \"\n                      :title=\"setStatus(order.order_status.value).text\"\n                      @click=\"checkUpdateStatus(order.order_status.value)\"\n                    >\n                      <span class=\"material-icons mr-2\">{{\n                        setStatus(order.order_status.value).icon\n                      }}</span>\n                      {{ setStatus(order.order_status.value).text }}\n                    </button>\n                    <button\n                      class=\"\n                        col-12\n                        btn btn-danger btn-status-order\n                        d-flex\n                        justify-content-center\n                        align-items-center\n                        mt-3\n                      \"\n                      v-if=\"\n                        order.order_status.value >= 1 &&\n                        order.order_status.value < 3 &&\n                        order.status[1] !== 'CANCELED'\n                      \"\n                      title=\"Cancelar Pedido\"\n                      @click=\"checkCanceledStatus(order.guid)\"\n                    >\n                      <span class=\"material-icons mr-2\"\n                        >remove_shopping_cart</span\n                      >\n                      Cancelar Pedido\n                    </button>\n                    <button\n                      class=\"\n                        col-12\n                        btn btn-warning btn-status-order\n                        d-flex\n                        justify-content-center\n                        align-items-center\n                        mt-3\n                      \"\n                      title=\"Imprimir Pedido\"\n                      @click=\"goPrint()\"\n                    >\n                      <span class=\"material-icons mr-2\">print</span>\n                      Imprimir Pedido\n                    </button>\n                  </template>\n\n                  <template v-else>\n                    <h4\n                      class=\"text-center text-danger\"\n                      :class=\"{\n                        'text-success': order.status[1] !== 'CANCELED',\n                        'text-danger': order.status[1] === 'CANCELED',\n                      }\"\n                      :title=\"\n                        'Esse pedido foi' + order.status[1] === 'CANCELED'\n                          ? 'cancelado'\n                          : 'finalizado'\n                      \"\n                    >\n                      {{\n                        order.status[1] === \"CANCELED\"\n                          ? \"Pedido Cancelado\"\n                          : \"Pedido Finalizado\"\n                      }}\n                    </h4>\n                  </template>\n\n                  <button\n                    id=\"myText\"\n                    class=\"\n                      col-12\n                      btn btn-primary\n                      d-flex\n                      justify-content-center\n                      align-items-center\n                      mt-2\n                      pulse\n                    \"\n                    @click=\"generateLink()\"\n                    v-if=\"order.order_status.value === 3\"\n                  >\n                    <img\n                      class=\"mr-2\"\n                      src=\"../../../assets/images/shipping-white.svg\"\n                      alt=\"update\"\n                      width=\"20\"\n                    />\n                    Entrega por Terceiros\n                  </button>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </section>\n\n      <section\n        class=\"row mt-2 print-column-oriented col-oriented-margin\"\n        v-if=\"order.customer\"\n      >\n        <div class=\"col-md-6 col-sm-12 p-1\">\n          <div class=\"card shadow-sm\">\n            <div class=\"card-body\">\n              <h5 class=\"card-title font-weight-bold print-principal-info\">\n                Comprador\n              </h5>\n\n              <div class=\"mb-2 print-same-line\">\n                <p class=\"card-text font-weight-bold label-text\">Nome:</p>\n                <p class=\"card-text print-lbl-info\">\n                  {{ order.customer.name }}\n                </p>\n              </div>\n\n              <div class=\"row\">\n                <div class=\"col-sm-12 col-lg-6 mb-2 print-same-line\">\n                  <p class=\"card-text font-weight-bold label-text\">Telefone:</p>\n                  <p class=\"card-text print-lbl-info\">\n                    <input\n                      id=\"customerPhone\"\n                      class=\"notInput card-text\"\n                      readonly\n                      type=\"tel\"\n                      name=\"customerPhone\"\n                      v-model=\"order.customer.phone\"\n                      v-mask=\"'(##) #####-####'\"\n                    />\n                  </p>\n                </div>\n                <div class=\"col-sm-12 col-lg-6 mb-2 print-same-line\">\n                  <p class=\"card-text font-weight-bold label-text\">E-mail:</p>\n                  <p class=\"card-text print-lbl-info\">\n                    {{ order.customer.email }}\n                  </p>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"col-md-6 col-sm-12 p-1\">\n          <div class=\"card shadow-sm card-customer\">\n            <div class=\"card-body\">\n              <h5 class=\"card-title font-weight-bold print-principal-info\">\n                Pagamento\n              </h5>\n\n              <div class=\"mb-2 print-same-line\">\n                <p class=\"card-text font-weight-bold label-text\">\n                  Forma de pagamento:\n                </p>\n                <p class=\"card-text print-lbl-info\">\n                  {{ order.payment_method }}\n                </p>\n              </div>\n\n              <div class=\"mb-2 print-same-line hidden-print\">\n                <p class=\"card-text font-weight-bold label-text\">\n                  Valor do pedido:\n                </p>\n                <p class=\"card-text print-lbl-info\">\n                  <money\n                    class=\"notInput card-text font-weight-bold\"\n                    v-model=\"order.order_price\"\n                    v-bind=\"moneyCash\"\n                    disabled\n                  ></money>\n                </p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </section>\n\n      <div class=\"row\">\n        <div class=\"col-12 p-1\">\n          <div class=\"card shadow-sm\" v-if=\"order.shipping\">\n            <div class=\"card-body\">\n              <h5\n                class=\"\n                  card-title\n                  font-weight-bold\n                  d-flex\n                  justify-content-between\n                  print-principal-info\n                \"\n              >\n                Entrega\n              </h5>\n\n              <div class=\"mb-2 print-same-line\">\n                <p class=\"card-text font-weight-bold label-text\">Nome:</p>\n                <p class=\"card-text print-order-margins print-lbl-info\">\n                  {{ order.customer.name }}\n                </p>\n              </div>\n\n              <div class=\"row print-column-oriented\">\n                <div class=\"col-sm-12 col-md-3 mb-2 print-same-line\">\n                  <p class=\"card-text font-weight-bold label-text\">CEP:</p>\n                  <p class=\"card-text print-order-cep-info print-lbl-info\">\n                    {{ order.shipping.shipping_postalcode }}\n                  </p>\n                </div>\n\n                <div class=\"col-sm-12 col-md-9 mb-2 print-address-info\">\n                  <p class=\"card-text font-weight-bold label-text\">Endereço:</p>\n                  <p class=\"card-text print-lbl-info\">\n                    {{\n                      order.shipping.shipping_complete_address +\n                      \" - \" +\n                      formatCountry(order.shipping.shipping_country)\n                    }}\n                  </p>\n                </div>\n              </div>\n\n              <hr v-if=\"order.shipping.shipment_type\" class=\"hidden-print\" />\n\n              <div v-if=\"order.shipping.shipment_type\">\n                <h6 class=\"card-title font-weight-bold\">\n                  {{ order.shipping.shipment_type }}\n                </h6>\n                <p\n                  class=\"card-text print-lbl-info\"\n                  v-if=\"order.shipping.shipment_type == 'Entrega expressa'\"\n                >\n                  {{ formatEstimatedTime(order.shipping_estimated_time) }}\n                </p>\n              </div>\n\n              <hr v-if=\"order.comments\" />\n\n              <div v-if=\"order.comments\">\n                <h5 class=\"card-title font-weight-bold\">Comentários</h5>\n                <p class=\"card-text print-lbl-info\">{{ order.comments }}</p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"row my-4 print-end\" v-if=\"order.items\">\n        <div class=\"col-12 p-2\">\n          <h5 class=\"card-title font-weight-bold print-principal-info\">\n            Produtos\n          </h5>\n\n          <ul class=\"list-group my-list mt-2\">\n            <li\n              class=\"list-group-item my-list-item p-1 mb-2 mx-0 d-flex flex-row\"\n              v-for=\"product in order.items\"\n            >\n              <div class=\"product-img mr-2 hidden-print\">\n                <img :src=\"product.image_url\" alt=\"Logo\" />\n              </div>\n\n              <div class=\"product-description\">\n                <h6 class=\"card-title font-weight-bold m-0\">\n                  {{ product.name }}\n                </h6>\n                <small\n                  class=\"variant hidden-print\"\n                  v-if=\"product.variant_name && product.variant_value\"\n                >\n                  {{ product.variant_name + \": \" + product.variant_value }}\n                </small>\n\n                <div class=\"d-flex flex-row justify-content-between\">\n                  <p class=\"card-text print-products-info\">\n                    {{ product.quantity }}x\n                  </p>\n                  <money\n                    class=\"\n                      card-text\n                      text-right\n                      font-weight-bold\n                      notInput\n                      print-products-info\n                    \"\n                    v-model=\"product.price\"\n                    v-bind=\"money\"\n                    disabled\n                  ></money>\n                </div>\n\n                <template v-if=\"product.comments\" class=\"hidden-print\">\n                  <h6 class=\"card-title font-weight-bold\">Comentários:</h6>\n                  <p class=\"card-text\">{{ product.comments }}</p>\n                </template>\n              </div>\n            </li>\n          </ul>\n        </div>\n      </div>\n\n      <div class=\"row my-5 print-end\">\n        <div class=\"col-12 p-2\">\n          <p\n            class=\"\n              card-text\n              mb-8\n              d-flex\n              flex-row\n              justify-content-between\n              print-title\n            \"\n          >\n            Subtotal\n            <money\n              class=\"card-text text-right font-weight-bold notInput\"\n              v-model=\"order.subtotal\"\n              v-bind=\"money\"\n              disabled\n            ></money>\n          </p>\n          <p\n            class=\"\n              card-text\n              d-flex\n              flex-row\n              justify-content-between\n              print-title\n            \"\n            v-if=\"order.shipping\"\n          >\n            Frete\n            <money\n              class=\"card-text text-right font-weight-bold notInput\"\n              v-model=\"order.shipping.shipment_price\"\n              v-bind=\"money\"\n              disabled\n            ></money>\n          </p>\n\n          <hr />\n\n          <p\n            class=\"\n              card-text\n              d-flex\n              flex-row\n              justify-content-between\n              font-weight-bold\n              print-title print-principal-info\n            \"\n          >\n            Total\n            <money\n              class=\"card-text text-right font-weight-bold notInput\"\n              v-model=\"order.order_price\"\n              v-bind=\"money\"\n              disabled\n            ></money>\n          </p>\n        </div>\n      </div>\n    </section>\n  </section>\n</template>\n\n<script lang=\"ts\">\n  import { Component, Prop, Vue } from \"vue-property-decorator\";\n  import { VMoney } from \"v-money\";\n  import VueSimpleAlert from \"vue-simple-alert\";\n  import Swal, { SweetAlertType } from \"sweetalert2\";\n  import moment from \"moment\";\n\n  import OrderService from \"@/services/orders.service\";\n  import { Order } from \"@/models/order\";\n\n  @Component({\n    directives: { money: VMoney },\n  })\n  export class Tracking extends Vue {\n    private orderService = new OrderService();\n\n    @Prop({ required: true }) public propOrder!: Order;\n    @Prop({ required: true }) public propOrderNumber!: string;\n    @Prop({ required: true }) public propVerification!: boolean;\n    @Prop({ required: true }) public propLoading!: boolean;\n\n    @Prop({ default: true, required: false }) public propTitle!: boolean;\n\n    @Prop() propPrevDisabled!: boolean;\n\n    public order: Order = this.propOrder;\n    public orderNumber: string = this.propOrderNumber;\n    public verification: boolean = this.propVerification;\n    public loading: boolean = this.propLoading;\n    public viewTitle: boolean = this.propTitle;\n    public title = \"Acompanhamento do Pedido\";\n    public money = {\n      decimal: \",\",\n      thousands: \".\",\n      prefix: \"R$ \",\n      suffix: \"\",\n      precision: 2,\n      masked: false,\n    };\n    public moneyCash = {\n      decimal: \",\",\n      thousands: \".\",\n      prefix: \"R$ \",\n      suffix: \" à vista\",\n      precision: 2,\n      masked: false,\n    };\n    public statusItems = [\n      { name: \"Pedido Efetuado\", status: \"CREATED\" },\n      { name: \"Pedido recebido pelo vendedor\", status: \"RECEIVED SELLER\" },\n      { name: \"Pedido saiu para entrega\", status: \"READY DELIVERY\" },\n      { name: \"Pedido entregue\", status: \"DELIVERED\" },\n    ];\n\n    constructor() {\n      super();\n      document.title = this.title;\n    }\n\n    private async updateStatus(statusNumber: number) {\n      await this.orderService\n        .updateOrder(this.orderNumber, statusNumber)\n        .then((data) => {\n          if (data.ok) {\n            VueSimpleAlert.fire({\n              title: this.setNewStatus(this.order.order_status?.label),\n              text: \"Status do Pedido Atualizado!\",\n              type: \"success\",\n              timer: 3000,\n            }).then(() =>\n              data\n                .json()\n                .then((order) => (this.order.order_status = order.order_status))\n            );\n          } else {\n            VueSimpleAlert.fire({\n              title: \"Erro ao alterar Status do Pedido\",\n              text: \"Não é possível alterar o status o pedido.\",\n              type: \"error\",\n              timer: 4000,\n            }).then(() => this.$router.push(\"dashboard\"));\n          }\n        })\n        .catch((err) => console.error(err));\n    }\n\n    public checkUpdateStatus(value: number) {\n      VueSimpleAlert.fire({\n        title: `${this.setStatus(value).text}?`,\n        text: `Tem certeza que deseja ${this.setStatus(value).text}?`,\n        type: \"warning\",\n        showCancelButton: true,\n        cancelButtonText: \"Não\",\n        confirmButtonText: \"Sim\",\n        customClass: {\n          title: \"title-modal\",\n          content: \"text-uppercase\",\n        },\n      }).then((result) => {\n        if (result.value) {\n          this.updateStatus(value + 1);\n        }\n      });\n    }\n\n    public goPrint() {\n      window.print();\n    }\n\n    public checkCanceledStatus(guid: string) {\n      VueSimpleAlert.fire({\n        title: `Cancelar Pedido?`,\n        text: `Tem certeza que deseja cancelar o pedido?`,\n        type: \"warning\",\n        showCancelButton: true,\n        cancelButtonText: \"Não\",\n        confirmButtonText: \"Sim\",\n        customClass: {\n          title: \"title-modal\",\n          content: \"text-uppercase\",\n        },\n      }).then((result) => {\n        if (result.value) {\n          this.cancelOrder(guid);\n        }\n      });\n    }\n\n    public cancelOrder = (orderGuid: string) =>\n      this.orderService\n        .cancelOrder(orderGuid)\n        .then(() => {\n          this.$emit(\"getOrder\", true);\n          if (this.order.status) {\n            this.order.status[1] = \"CANCELED\";\n          }\n        })\n        .catch(() =>\n          this.setToastAlert(\n            \"error\",\n            `No momento não foi possivel cancelar esse pedido.`,\n            4000\n          )\n        );\n\n    setToastAlert(type: SweetAlertType, title: string, timer: number) {\n      VueSimpleAlert.fire({\n        type,\n        title,\n        toast: true,\n        position: \"top-end\",\n        showConfirmButton: false,\n        timer,\n        onOpen: (toast) => {\n          toast.addEventListener(\"mouseenter\", Swal.stopTimer),\n            toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n        },\n      });\n    }\n\n    public generateLink() {\n      VueSimpleAlert.fire({\n        html: `\n                  <div class=\"col-12 mb-3\">\n                      <h4 class=\"m-0 font-weight-bold\">Copie o link e envie ao responsável pela entrega</h4>\n                  </div>\n                  <div class=\"input-group mb-3\">\n                      <input type=\"text\" id=\"swal-input1\" class=\"form-control\" readonly>\n                      <div class=\"input-group-append\">\n                          <button id=\"copy\" class=\"btn btn-outline-secondary\" type=\"button\">\n                              Copiar\n                          </button>\n                      </div>\n                  </div>\n              `,\n        inputValue: \"\",\n        onOpen: () => {\n          const pertinho = JSON.parse(localStorage.getItem(\"pertinho\") || \"{}\");\n          const link = `https://www.pertinhodecasa.com.br/meus-pedidos/${pertinho.guid}/${this.orderNumber}`;\n          const content = Swal.getContent();\n          const input1 = content.querySelector(\n            \"#swal-input1\"\n          ) as HTMLInputElement;\n          const confirm = content.querySelector(\"#copy\") as HTMLInputElement;\n          if (input1) {\n            input1.value = link;\n            confirm.addEventListener(\"click\", (e) => {\n              if (e) {\n                const copyText = document.getElementById(\n                  \"swal-input1\"\n                ) as HTMLInputElement;\n                copyText.select();\n                copyText.setSelectionRange(0, 99999);\n                document.execCommand(\"copy\");\n                VueSimpleAlert.fire({\n                  type: \"success\",\n                  title: \"Link copiado para área de transferência\",\n                  toast: true,\n                  position: \"top-end\",\n                  showConfirmButton: false,\n                  timer: 3000,\n                  onOpen: (toast) => {\n                    toast.addEventListener(\"mouseenter\", Swal.stopTimer),\n                      toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n                  },\n                });\n              }\n            });\n          }\n        },\n        showCancelButton: false,\n        showConfirmButton: false,\n      });\n    }\n\n    public formatDate(data: Date) {\n      if (data) {\n        return moment(String(data)).utc().locale(\"pt-br\").format(\"LL\");\n      }\n    }\n\n    public formatEstimatedTime(time: string) {\n      if (time) {\n        switch (time) {\n          case \"00:00:00\":\n            return \"à combinar\";\n          case \"01:00:00\":\n            return \"1 hora\";\n          case \"23:59:59\":\n          case \"1d\":\n            return \"1 dia\";\n        }\n      }\n    }\n\n    public formatCountry(country: string) {\n      if (country) {\n        switch (country) {\n          case \"BR\":\n            return \"Brasil\";\n          case \"US\":\n            return \"Estados Unidos\";\n          default:\n            return country;\n        }\n      }\n    }\n\n    public setClass = (myClass: string): string =>\n      myClass.toLowerCase().replace(\" \", \"_\");\n\n    public setNewStatus(status: string | undefined) {\n      switch (status) {\n        case \"CREATED\":\n          return \"Pedido recebido pelo vendedor\";\n        case \"RECEIVED SELLER\":\n          return \"Pedido saiu para entrega\";\n        case \"READY DELIVERY\":\n          return \"Pedido entregue\";\n        case \"DELIVERED\":\n          return \"Pedido será finalizado\";\n      }\n    }\n\n    public setStatus(status: number) {\n      const nameStatus = { text: \"\", icon: \"\" };\n      switch (status) {\n        case 1:\n          nameStatus.text = \"Confirmar recebimento do pedido\";\n          nameStatus.icon = \"local_mall\";\n          break;\n        case 2:\n          nameStatus.text = \"Avisar que o pedido saiu para entrega\";\n          nameStatus.icon = \"local_shipping\";\n          break;\n        case 3:\n          nameStatus.text = \"Confirmar entrega do pedido\";\n          nameStatus.icon = \"cottage\";\n          break;\n        default:\n          break;\n      }\n      return nameStatus;\n    }\n  }\n\n  export { Tracking as default };\n</script>\n\n<style lang=\"scss\" scoped>\n  @media print {\n    .hidden-print,\n    .hidden-print * {\n      display: none !important;\n    }\n\n    .print-principal-info {\n      text-transform: uppercase;\n    }\n\n    .card-order {\n      height: 120% !important;\n\n      .divisor {\n        display: none;\n      }\n\n      p {\n        color: black !important;\n        font-weight: normal !important;\n        font-size: 43px;\n      }\n\n      .text-white {\n        color: black !important;\n        font-weight: bold;\n      }\n    }\n\n    .print-column-oriented {\n      display: flex;\n      flex-direction: column;\n    }\n    \n    .col-oriented-margin {\n      margin-left: -4%;\n    }\n\n    .print-same-line {\n      display: flex;\n      flex-direction: row;\n    }\n\n    .card {\n      border: none !important;\n      box-shadow: none !important;\n\n      .card-body {\n        margin-left: -5%;\n        width: 90% !important;\n\n        .card-title {\n          color: black !important;\n          font-size: 45px;\n        }\n\n        .print-lbl-info {\n          color: black !important;\n          font-weight: normal;\n          font-size: 45px;\n          margin-right: -20% !important;\n        }\n\n        .label-text {\n          color: black !important;\n          font-weight: normal;\n          font-size: 45px;\n        