-0\" @click=\"openHelpModal(inputs.id)\">\n            <span class=\"material-icons\">help_outline</span>\n          </button>\n          <div class=\"card card-help\" v-if=\"showHelpModal && toastHelp === inputs.id\">\n            <div class=\"card-body\">\n              <p class=\"m-0\" v-html=\"inputs.toastHelp\"></p>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <input v-if=\"inputs.tag === 'input'\" :type=\"inputs.type\" :class=\"inputs.classInput\" :id=\"inputs.id\"\n        :aria-describedby=\"inputs.ariaDescribedby\" :placeholder=\"inputs.placeholder\" :required=\"inputs.required\"\n        :disabled=\"inputs.disabled\" v-model=\"inputs.model\" :maxlength=\"inputs.maxlength\" @input=\"validateInputs\"\n        @blur=\"blurInputs\" @focus=\"focusInput\" autocomplete=\"no\" />\n\n      <template v-if=\"inputs.tag === 'radio'\">\n        <div v-for=\"radio in inputs.radio\" :class=\"inputs.classContainer\">\n          <input :type=\"radio.type\" :class=\"radio.classInput\" :id=\"radio.id\" :value=\"radio.value\" v-model=\"inputs.model\"\n            @input=\"validateInputs\" />\n          <label :class=\"radio.classLabel\" :for=\"radio.id\">\n            {{ radio.label }}\n          </label>\n        </div>\n      </template>\n\n      <textarea v-if=\"inputs.tag === 'textarea'\" :class=\"inputs.classInput\" :id=\"inputs.id\"\n        :aria-describedby=\"inputs.ariaDescribedby\" :placeholder=\"inputs.placeholder\" :required=\"inputs.required\"\n        v-model=\"inputs.model\" @input=\"validateInputs\" autocomplete=\"off\"></textarea>\n\n      <multiselect v-if=\"inputs.tag === 'select' && !inputs.optionGroups\" v-model=\"inputs.model\" @input=\"validateInputs\"\n        :options=\"inputs.options\" :label=\"inputs.selectLabel\" :track-by=\"inputs.selectLabel\"\n        :placeholder=\"inputs.placeholder\" deselect-label=\"Remover Selecionado\" selectedLabel=\"Selecionado\"\n        :select-label=\"inputs.placeholder\">\n        <span slot=\"noResult\" v-html=\"inputs.noResult\"></span>\n        <span slot=\"noOptions\" v-html=\"inputs.noOptions\"></span>\n      </multiselect>\n\n      <multiselect v-if=\"inputs.tag === 'select' && inputs.optionGroups\" v-model=\"inputs.model\" @input=\"validateInputs\"\n        :options=\"inputs.optionGroups\" :multiple=\"false\" :group-select=\"false\" group-values=\"options\"\n        :group-label=\"selectLabel\" :label=\"inputs.selectLabel\" :track-by=\"inputs.selectLabel\"\n        :placeholder=\"inputs.placeholder\" :noOptions=\"inputs.noOptions\" deselect-label=\"Remover Selecionado\"\n        selectedLabel=\"Selecionado\" :select-label=\"inputs.placeholder\">\n        <span slot=\"noResult\" v-html=\"inputs.msgNoResult\"></span>\n        <template slot=\"singleLabel\" slot-scope=\"{ option }\">\n          {{ inputs.labelResult }} <strong>{{ option.category }}</strong>\n        </template>\n      </multiselect>\n\n      <small :id=\"inputs.ariaDescribedby\" :class=\"inputs.classHelp\">\n        {{ inputs.help }}\n      </small>\n    </div>\n  </section>\n</template>\n\n<script lang=\"ts\">\nimport { Component, Prop, Vue } from \"vue-property-decorator\";\nimport { InputForms } from \"@/models/inputForms\";\nimport Multiselect from \"vue-multiselect\";\nimport \"vue-multiselect/dist/vue-multiselect.min.css\";\n\n@Component({\n  components: {\n    Multiselect,\n  },\n})\nexport class InputForm extends Vue {\n  @Prop() propArrayInputs!: InputForms[];\n\n  public arrayInputs = this.propArrayInputs;\n  public showHelpModal = false;\n  public toastHelp = \"\";\n\n  public validateInputs() {\n    this.$emit(\"validateInputs\", this.arrayInputs);\n  }\n\n  public blurInputs() {\n    this.$emit(\"blurInputs\", true);\n  }\n\n  public focusInput() {\n    this.$emit(\"focusInput\", true);\n  }\n\n  public openHelpModal(label: string) {\n    this.showHelpModal = !this.showHelpModal;\n    this.toastHelp = label;\n  }\n}\n\nexport { InputForm as default };\n</script>\n\n<style lang=\"scss\" scoped>\n.card-help {\n  position: absolute;\n  height: auto;\n  z-index: 99;\n  box-shadow: 0 9px 15px -3px rgba(43, 88, 164, 0.23);\n  text-align: justify;\n}\n\n@media screen and (min-width: 200px) and (max-width: 520px) {\n  .card-help {\n    right: 0;\n    width: 300px !important;\n  }\n\n}\n\n\n\n@media screen and (max-width: 2000px) {\n  .inputs-label {\n    justify-content: space-between;\n  }\n\n  .card-help {\n    right: 0;\n    width: 500px;\n  }\n\n}\n</style>"]}]}