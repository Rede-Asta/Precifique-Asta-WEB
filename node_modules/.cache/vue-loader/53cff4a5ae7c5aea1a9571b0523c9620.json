 :active=\"showMapAddress\">\n    <section class=\"card card-map\">\n      <div class=\"card-body d-flex flex-column align-items-center\">\n        <section class=\"card-titles\">\n          <h2 class=\"card-title text-uppercase\">{{ title }}</h2>\n          <p class=\"card-subtitle\">{{ subtitle }}</p>\n        </section>\n\n        <div class=\"map flex-grow-1\" ref=\"map\"></div>\n\n        <div\n          class=\"\n            d-flex\n            align-items-center\n            justify-content-center\n            mt-2\n            confirm-button-container\n          \"\n        >\n          <button class=\"btn btn-primary w-100\" @click=\"confirmAddressMap()\">\n            Confirmar\n          </button>\n        </div>\n      </div>\n    </section>\n  </modal>\n</template>\n\n<script lang=\"ts\">\n  import { Component, Prop, Vue } from \"vue-property-decorator\";\n\n  declare global {\n    interface Window {\n      google: any;\n    }\n  }\n\n  @Component({})\n  export class MapAddress extends Vue {\n    @Prop() propShowMapAddress!: boolean;\n\n    public title: string = \"Confirme seu endereço\";\n    public subtitle: string =\n      \"Clique no ponto do mapa mais próximo à sua localização e confirme seu endereço.\";\n\n    public showMapAddress: boolean = this.propShowMapAddress;\n\n    mounted() {\n      this.initMap();\n    }\n\n    private initMap() {\n      let myLatlng = JSON.parse(localStorage.getItem(\"latlng\") || \"\");\n      const map = new window.google.maps.Map(this.$refs.map, {\n        center: myLatlng,\n        zoom: 17,\n        mapTypeControl: false,\n        scaleControl: false,\n        streetViewControl: false,\n        rotateControl: false,\n        fullscreenControl: false,\n        overviewMapControl: false,\n      });\n      let infoWindow = this.placeInfoWindow(myLatlng);\n      let markerUser = this.placeMarkerUser(myLatlng, map);\n\n      const setMarkerPos = (myLatlng) => {\n        let center = map.getCenter();\n        myLatlng = { lat: center.lat(), lng: center.lng() };\n        localStorage.setItem(\"latlng\", JSON.stringify(myLatlng));\n        markerUser.setPosition(center);\n        infoWindow.setPosition(center);\n      };\n\n      map.addListener(\"drag\", (event) => {\n        setMarkerPos(myLatlng);\n      });\n\n      map.addListener(\"dblclick\", (event) => {\n        setMarkerPos(myLatlng);\n      });\n\n      map.addListener(\"zoom_changed\", (event) => {\n        setMarkerPos(myLatlng);\n      });\n\n      infoWindow.open(map);\n    }\n\n    private placeInfoWindow(myLatlng: any) {\n      return new window.google.maps.InfoWindow({\n        content: `\n                  <h6 class=\"text-center font-weight-bold m-0\">Esta é localização do negócio?</h6>\n                  <p class=\"text-center m-0 text-muted\">Se for necessário ajuste sua localização</p>\n              `,\n        position: myLatlng,\n      });\n    }\n\n    private placeMarkerUser(myLatlng: any, map) {\n      return new window.google.maps.Marker({\n        position: myLatlng,\n        map,\n        title: \"Estou aqui\",\n        icon: {\n          url: require(\"@/assets/images/icon-place.svg\"),\n          size: new window.google.maps.Size(50, 50),\n          origin: new window.google.maps.Point(0, 0),\n          anchor: new window.google.maps.Point(24, 10),\n          scaledSize: new window.google.maps.Size(50, 50),\n        },\n      });\n    }\n\n    public confirmAddressMap() {\n      this.$emit(\"confirmAddressMap\", true);\n    }\n\n    public closeMapAddress() {\n      this.$emit(\"closeMapAddress\", true);\n    }\n  }\n\n  export { MapAddress as default };\n</script>\n\n<style lang=\"scss\" scoped>\n  .card-map {\n    width: 55vw;\n    border-radius: 6px;\n    border: none;\n\n    @media screen and (max-width: 732px) {\n      width: 100vw;\n      height: 85vh;\n      border-radius: 0px;\n    }\n    \n    @media screen and (min-width: 733px) and (max-width: 992px) {\n      width: 95vw;\n    }\n\n    @media screen and (min-width: 993px) and (max-width: 1200px) {\n      width: 90vw;\n    }\n\n    @media screen and (min-width: 1201px) and (max-width: 1400px) {\n      width: 80vw;\n    }\n\n    @media screen and (min-width: 1401px) and (max-width: 1600px) {\n      width: 55vw;\n    }\n\n    .card-body {\n      height: 85vh;\n    }\n\n    .card-titles {\n      margin-bottom: 1rem;\n    }\n\n    .card-title,\n    .card-subtitle {\n      font-family: Raleway;\n      font-stretch: normal;\n      font-style: normal;\n      line-height: normal;\n      letter-spacing: normal;\n      color: #525252;\n      text-align: center;\n    }\n\n    .card-title {\n      font-size: 28px;\n      font-weight: bold;\n\n      @media screen and (max-width: 992px) {\n        font-size: 1.5rem;\n      }\n    }\n\n    .card-subtitle {\n      @media screen and (max-width: 992px) {\n        font-size: 0.5rem;\n      }\n    }\n\n    .card-subtitle {\n      font-size: 20px;\n      font-weight: normal;\n      color: #525252;\n\n      @media screen and (max-width: 992px) {\n        font-size: 16px;\n      }\n    }\n  }\n\n  .map {\n    width: 100%;\n    //height: 50vh;\n    border-radius: 6px;\n    position: relative;\n\n    @media screen and (max-width: 992px) {\n      width: 100%;\n      //height: 65vh;\n    }\n  }\n\n  .confirm-button-container {\n    position: absolute;\n    bottom: 40px;\n\n    left: 50%;\n    transform: translate(-50%);\n\n    button {\n      padding-left: 20px;\n      padding-right: 20px;\n    }\n  }\n\n  .my-close-map {\n    position: absolute;\n    top: -14px;\n    right: -14px;\n    background-color: #950ac6;\n    width: 40px;\n    height: 40px;\n    border-radius: 50%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    border: 3px solid #fff;\n    cursor: pointer;\n    transition: ease-in-out 1s;\n    z-index: 99;\n\n    .fas {\n      color: #ffffff;\n    }\n\n    &:hover {\n      transition: ease-in-out 2s;\n      background-color: #ffffff;\n\n      .fas {\n        color: #950ac6;\n      }\n    }\n\n    @media screen and (max-width: 992px) {\n      top: 6px;\n      right: 6px;\n    }\n  }\n</style>"]}]}