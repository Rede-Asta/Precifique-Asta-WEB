"ts\">\nimport Vue, {PropType} from 'vue';\nimport {IProduct} from \"@/models/statistics\";\nimport environment from \"@/environments/environment\";\n\nexport default Vue.extend({\n  name: \"CardBestSellerProduct\",\n\n  computed: {\n    productImage(): string {\n      const firstImage = this.product.img_list[0];\n      return firstImage ? firstImage.img_url : `${environment.s3BaseUrlProducts}/sem_imagem_vendedor.png`;\n    }\n  },\n\n  props: {\n    product: {\n      type: Object as PropType<IProduct>,\n      required: true,\n      default: () => ({} as IProduct)\n    }\n  },\n\n  data() {\n    return {\n      s3BrokenImgPath: environment.s3BrokenImgPath,\n      s3DefaultImgPath: `${environment.s3BaseUrlProducts}/ilustracao_default.jpg`,\n    }\n  }\n});\n</script>\n\n<style scoped lang=\"scss\">\n.best-seller-product {\n  width: 215px;\n\n  background-color: #fff;\n\n  box-shadow: 0px 0px 8px 2px rgba(0, 0, 0, 0.05);\n  border-radius: 10px;\n\n  padding: .75rem;\n\n  line-height: 1;\n\n  img {\n    width: 60px;\n    height: 60px;\n\n    object-fit: contain;\n\n    box-shadow: 0px 0px 8px 2px rgba(0, 0, 0, 0.05);\n    border-radius: 7px;\n  }\n\n  &__name {\n    display: -webkit-box;\n    -webkit-box-orient: vertical;\n    -moz-box-orient: vertical;\n\n    color: #525252;\n    font-size: 0.6875rem;\n    font-weight: bold;\n\n    line-height: 1.2;\n\n    height: fit-content;\n    max-width: 108px;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    line-clamp: 2;\n    -webkit-line-clamp: 2;\n    word-break: break-all;\n  }\n\n  &__price {\n    font-size: .75rem;\n    color: $success;\n  }\n\n  &__sold-count {\n    font-size: .625rem;\n    color: #525252;\n  }\n}\n</style>"]}]}