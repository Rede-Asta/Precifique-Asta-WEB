lass=\"share-card--btn\" @click=\"openShareLinkModal = true;\">\n          <img src=\"@/assets/images/social-network/icon-copy.svg\" alt=\"\" srcset=\"\" style=\"margin-right: 5px;\">\n          <strong>Copiar Link</strong>\n        </button>\n        <button class=\"share-card--btn\" @click=\"complinkLoja\">\n          <img src=\"@/assets/images/loja.svg\" alt=\"\" srcset=\"\" style=\"margin-right: 5px;\">\n          <strong>Link da loja</strong>\n        </button>\n        <button class=\"share-card--btn\" @click=\"compWhatsapp\">\n          <img src=\"@/assets/images/whatsapp.svg\" alt=\"\" srcset=\"\" style=\"margin-right: 5px;\">\n          <strong>WhatsApp</strong>\n        </button>\n        <button class=\"share-card--btn\" @click=\"compFacebook\">\n          <img src=\"@/assets/images/social-network/icon-facebook.svg\" alt=\"\" srcset=\"\"\n               style=\"margin-right: 10px; margin-left: 5px;\">\n          <strong>Facebook</strong>\n        </button>\n        <button class=\"share-card--btn\" @click=\"compMessenger\">\n          <img src=\"@/assets/images/social-network/icon-facebook-messenger.svg\" alt=\"\" srcset=\"\"\n               style=\"margin-right: 5px;\">\n          <strong>Messenger</strong>\n        </button>\n        <button class=\"share-card--btn\" @click=\"compTwitter\">\n          <img src=\"@/assets/images/social-network/icon-twitter.svg\" alt=\"\" srcset=\"\" style=\"margin-right: 5px;\">\n          <strong>Twitter</strong>\n        </button>\n      </div>\n    </modal>\n\n    <modal :active=\"openShareLinkModal\">\n      <div class=\"share-card\">\n        <img class=\"share-card--title\" src=\"@/assets/images/close.svg\" alt=\"\" @click=\"openShareLinkModal = false\"\n             style=\"float:right;margin-bottom: 10px;\">\n        <input type=\"text\" id=\"shareTextInput\" v-model=\"storeShareLink\" readonly>\n        <button class=\"share-card--btn \"\n                style=\"margin-top: 5px; background-color: $primary; color: white; text-align: center;\"\n                @click=\"copyLink();\">\n          <strong>Copiar link</strong>\n        </button>\n      </div>\n    </modal>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport Vue from 'vue';\n\nimport getUtm from '@/helpers/getUtm';\n\nexport default Vue.extend({\n  name: \"ShareModal\",\n  props: ['value'],\n\n  data() {\n    return {\n      loggedUser: null,\n      openShareLinkModal: false,\n\n      storeShareLink: '',\n    }\n  },\n\n  mounted() {\n    this.getLoggedUser();\n    this.setStoreShareLink();\n  },\n\n  methods: {\n    setStoreShareLink() {\n      // @ts-ignore: next-line\n      this.storeShareLink = `https://www.pertinhodecasa.com.br/vitrine/${this.loggedUser.slug}?${getUtm('link', false)}`\n    },\n\n    getLoggedUser() {\n      if (localStorage.getItem('loggedUser')) {\n        this.loggedUser = JSON.parse(localStorage.getItem('loggedUser') || '');\n      }\n    },\n\n    copyLink() {\n      const copyText = document.getElementById('shareTextInput');\n      (copyText as HTMLInputElement).select();\n      (copyText as HTMLInputElement).setSelectionRange(0, 99999);\n      document.execCommand('copy');\n      alert('Link copiado!');\n    },\n\n    complinkLoja() {\n      // @ts-ignore: next-line\n      window.open(`https://www.pertinhodecasa.com.br/vitrine/${this.loggedUser.slug}?${getUtm('link', false)}`);\n    },\n\n    compWhatsapp() {\n      // @ts-ignore: next-line\n      window.open(`https://api.whatsapp.com/send?text=Ei,%20pessoal!%20Conheçam%20a%20minha%20loja%20dentro%20da%20Pertinho%20de%20Casa!%20Compre%20no%20site%20ou%20pelo%20app!%0Ahttps://www.pertinhodecasa.com.br/vitrine/${this.loggedUser.slug}?${getUtm('whatsapp')}`);\n    },\n\n    compFacebook() {\n      // @ts-ignore: next-line\n      window.open(`https://www.facebook.com/sharer/sharer.php?u=http%3A%2F%2Fwww.pertinhodecasa.com.br%2Fvitrine%2F${this.loggedUser.slug}?${getUtm('facebook')}&src=sdkpreparse`);\n    },\n\n    compMessenger() {\n      // @ts-ignore: next-line\n      window.open(`https://www.facebook.com/login.php?u=http%3A%2F%2Fwww.pertinhodecasa.com.br%2Fvitrine%2F${this.loggedUser.slug}?${getUtm('messenger')}&src=sdkpreparse`);\n    },\n\n    compTwitter() {\n      // @ts-ignore: next-line\n      window.open(`https://twitter.com/intent/tweet?url=https://www.pertinhodecasa.com.br/vitrine/${this.loggedUser.slug}?${getUtm('twitter')}&text=Ei,%20pessoal!%20Conheçam%20a%20minha%20loja%20dentro%20da%20Pertinho%20de%20Casa!%20Compre%20no%20site%20ou%20pelo%20app!`);\n    }\n  }\n});\n</script>\n\n<style scoped lang=\"scss\">\n.share-card {\n  background-color: white;\n  border-radius: 10px;\n  width: 300px;\n  height: auto;\n  padding: 10px;\n  color: rgb(255, 255, 255);\n  align-items: center;\n  justify-content: center;\n\n\n  &--title {\n    text-align: center;\n    font-size: 24px;\n    font-weight: bold;\n    color: rgb(78, 78, 78);\n  }\n\n  &--btn {\n    background-color: white;\n    border-style: solid;\n    border-color: rgb(226, 226, 226);\n    border-width: 1.5px;\n    border-radius: 10px;\n    margin-bottom: 10px;\n    padding: 10px 10px;\n    width: 100%;\n    height: 50px;\n    display: block;\n    text-align: left;\n    outline: none;\n\n    &:hover {\n      border-color: #585858;\n    }\n  }\n}\n</style>"]}]}