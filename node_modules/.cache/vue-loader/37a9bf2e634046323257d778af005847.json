none img-desk\" :src=\"banner.mobile\" alt=\"\">\n            </div>\n          </template>\n        </template>\n      </b-carousel-slide>\n    </b-carousel>\n\n    <div class=\"d-flex align-items-center justify-content-center mt-3\">\n      <div v-for=\"(banner, index) in banners\" class=\"banners-carousel__indicator mr-2\"\n        :class=\"{ 'banners-carousel__indicator--active': currentSlide === index }\" @click=\"currentSlide = index\">\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport Vue from 'vue';\nimport { BCarousel, BCarouselSlide } from \"bootstrap-vue\";\n\nexport default Vue.extend({\n  name: \"CoursesBannersCarousel\",\n\n  components: {\n    BCarousel,\n    BCarouselSlide,\n  },\n\n  props: ['banners'],\n\n  data() {\n    return {\n      currentSlide: 0,\n    }\n  }\n})\n</script>\n\n<style scoped lang=\"scss\">\n.banners-carousel {\n  .img-desk {\n    width: 100%;\n\n  }\n\n  &__indicator {\n    background-color: #c3c3c3;\n\n    $indicator-size: 15px;\n    height: $indicator-size;\n    width: $indicator-size;\n\n    border-radius: 50%;\n\n    cursor: pointer;\n\n    transition: background-color 0.2s ease;\n\n    &:last-child {\n      margin-right: 0 !important;\n    }\n\n    &--active {\n      background-color: $primary;\n    }\n  }\n}\n</style>"]}]}