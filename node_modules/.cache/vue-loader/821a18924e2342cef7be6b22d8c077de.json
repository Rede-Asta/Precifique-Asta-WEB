maxlength=\"maxLength\"\n        :disabled=\"disabled\"\n        @input=\"$emit('input', $event.target.value)\"\n      >\n    </template>\n\n    <div class=\"d-flex align-items-center justify-content-between mt-1 px-1\">\n      <span class=\"app-form-input__error-or-help\" v-if=\"error || help\">\n        {{ error || help }}\n      </span>\n      <span class=\"app-form-input__counter ml-auto\" v-if=\"maxLength\">\n        {{ value && value.length ? value.length : 0 }}/{{ maxLength }} caracteres\n      </span>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Vue from 'vue';\n\nexport default Vue.extend({\n  name: \"FormInput\",\n\n  props: {\n    help: {\n      type: String,\n    },\n\n    error: {\n      type: [String, Boolean],\n    },\n\n    value: {\n      type: String,\n    },\n\n    label: {\n      type: String,\n    },\n\n    name: {\n      type: String,\n    },\n\n    placeholder: {\n      type: String,\n    },\n\n    id: {\n      type: String,\n    },\n\n    textarea: {\n      type: Boolean,\n    },\n\n    maxLength: {\n      type: Number,\n    },\n\n    disabled: {\n      type: Boolean,\n    },\n\n    width: {\n      type: String,\n    },\n\n    maxWidth: {\n      type: String,\n    }\n  }\n});\n</script>\n\n<style scoped lang=\"scss\">\n.app-form-input {\n  display: flex;\n  flex-direction: column;\n\n  line-height: 1;\n\n  $app-form-input-selector: &;\n\n  &__label {\n    color: #535556E5;\n    font-size: .875rem;\n    font-weight: 700;\n    line-height: 1;\n\n    height: 1rem;\n  }\n\n  &__input {\n    width: 100%;\n    height: 34px;\n  }\n\n  &__textarea {\n    resize: none;\n    overflow: auto;\n    height: 100px;\n\n    padding-top: 9px !important;\n    padding-bottom: 9px !important;\n  }\n\n  &__input, &__textarea {\n    outline: none;\n\n    border-radius: 8px;\n    border: 0.8px solid rgba(0, 0, 0, 0.3);\n\n    padding: 0 9px;\n\n    transition: all 0.2s ease;\n\n    &::placeholder {\n      font-size: .75rem;\n      color: rgba(0, 0, 0, 0.4);\n    }\n\n    &:focus {\n      border-color: $primary;\n    }\n  }\n\n  &__counter {\n    font-size: .625rem;\n    color: rgba(0, 0, 0, 0.4);\n  }\n\n  &__error-or-help {\n    font-size: .75rem;\n    color: rgba(0, 0, 0, 0.4);\n  }\n\n  &.error {\n    #{$app-form-input-selector} {\n      &__input, &__textarea {\n        border-color: $error;\n\n        &:focus {\n          border-color: $error;\n        }\n      }\n\n      &__error-or-help, &__counter {\n        color: $error;\n      }\n    }\n  }\n}\n</style>"]}]}