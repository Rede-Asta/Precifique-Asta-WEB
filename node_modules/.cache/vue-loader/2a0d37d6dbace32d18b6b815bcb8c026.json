     @media screen and (min-width: 768px) {\n\n          overflow: hidden;\n          text-overflow: ellipsis;\n          display: -webkit-box;\n          -webkit-line-clamp: 4;\n          -webkit-box-orient: vertical;\n\n\n\n        }\n\n      }\n\n      p {\n        font-size: 14px;\n        font-weight: normal;\n        line-height: 1.2rem;\n        letter-spacing: 0.56px;\n        color: #525252;\n\n        overflow: hidden;\n        text-overflow: ellipsis;\n        display: -webkit-box;\n        -webkit-line-clamp: 6;\n        -webkit-box-orient: vertical;\n        text-align: start;\n\n\n      }\n    }\n\n    .card-courses-info {\n      li {\n        font-weight: 700;\n        font-size: 14px;\n\n        i {\n          text-align: center;\n          color: #950ac6;\n          font-size: 20px;\n          width: 25px;\n        }\n\n        &+li {\n          margin-top: 12px;\n        }\n      }\n    }\n  }\n\n  .card-courses-footer {\n    button {\n      border-radius: 35px;\n      border: none;\n      outline: none;\n\n      padding: 10px 20px;\n\n      background-color: #950ac6;\n\n      text-transform: uppercase;\n      color: #fff;\n\n      &:hover {\n        // background: #74049d;\n        // transition: 1s;\n        filter: brightness(90%)\n      }\n    }\n  }\n}\n\n@media screen and (min-width: 1400px) {\n  .container {\n    max-width: 1320px;\n  }\n}\n\na {\n  &:hover {\n    text-decoration: none;\n  }\n}\n\n.shinny-image {\n  height: 200px;\n}\n\n.shinny-title {\n  height: 20px;\n}\n\n.shinny-subtitle {\n  width: 80%;\n  height: 14px;\n}\n\n.shinny-icon {\n  display: inline-block;\n  width: 24px;\n  height: 24px;\n}\n\n.shinny-text {\n  width: 50%;\n  height: 14px;\n}\n\n.shinny-button {\n  width: 150px;\n  height: 50px;\n\n  border-radius: 35px;\n}\n\n.shinny {\n  position: relative;\n  background: #d3d3d3;\n  overflow: hidden;\n}\n\n.shinny:before {\n  content: \"\";\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  background: hsla(0, 0%, 100%, .2);\n  height: 100%;\n  transform: skew(5deg);\n  -webkit-animation: shine 1.5s ease-in-out infinite;\n  animation: shine 1.5s ease-in-out infinite\n}\n\n@-webkit-keyframes shine {\n  0% {\n    transform: skew(45deg) translateX(0)\n  }\n\n  to {\n    transform: skew(45deg) translateX(200%)\n  }\n}\n\n@keyframes shine {\n  0% {\n    transform: skew(45deg) translateX(0)\n  }\n\n  to {\n    transform: skew(45deg) translateX(200%)\n  }\n}\n\n.section-title {\n  font-size: 20px;\n  font-weight: bold;\n  color: #525252;\n}\n\n.margin {\n\n  margin-bottom: -3% !important;\n  margin-top: -2% !important;\n\n  @media screen and (max-width: 768px) {\n    margin: 0 !important;\n    margin-top: 7% !important;\n\n  }\n}\n.single-element{\n  margin-right: -4px !important;\n}\n</style>\n"]}]}