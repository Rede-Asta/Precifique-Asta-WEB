y-content-center\">\n      <button class=\"button-enter-code-precifique col-10 col-xl-5\" @click=\"submitForm()\">Enviar</button>\n    </div>\n  </section>\n\n  <login-precifique-code v-if=\"openNextPage\"\n    :propShowVerificationCode=\"showVerificationCode\"\n    :propEmail=\"email\"\n    :propOptIn=\"optIn\"/>\n</template>\n\n<script lang=\"ts\">\n\nimport { InputForms, TypeHelp } from \"@/models/inputForms\";\nimport RegisterSellerService from \"@/services/register.service\";\nimport { Component, Prop, Vue } from \"vue-property-decorator\";\nimport PrecifiqueService from \"@/services/precifiqueService\";\n\n\n@Component({})\nexport class LoginPrecifiqueForm extends Vue {\n  @Prop() propArrayInputs!: InputForms[];\n  @Prop() propEmail!: string;\n\n  public arrayInputs: InputForms[] = this.propArrayInputs;\n  public emailButton: boolean = false;\n\n  public email: string = this.propEmail;\n  public isValidEmail: boolean = true\n  public openNextPage: boolean = false\n\n\n  validateEmail() {\n    // Implemente a validação de email aqui\n    // Por exemplo, use uma expressão regular para validar o formato do email\n    const emailRegex = /^[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,4}$/;\n    this.isValidEmail = emailRegex.test(this.email);\n  }\n\n  public async submitForm() {\n    if (this.isValidEmail) {\n      this.openNextPage()\n      // setTimeout(() => {this.sendEmail()}, 10000);\n\n      const precifiqueService = new PrecifiqueService();\n      try {\n        await precifiqueService.sendEmail({\n          email: this.email,\n\n        });\n        this.$alert('Email enviado com sucesso', '', 'success',\n          {\n            toast: true,\n            position: 'top-end',\n            showConfirmButton: false,\n            timer: 3000,\n          });\n      } catch (e) {\n        console.error(e);\n        this.$alert('Erro ao enviar o email', '', 'error', {\n          toast: true,\n          position: 'top-end',\n          showConfirmButton: false,\n          timer: 3000,\n        });\n      }\n    }\n  }\n\n\n  public openNextPage() {\n  this.openNextPage = true;\n}\n\n\n\n}\n\nexport { LoginPrecifiqueForm as default };\n\n\n</script>\n\n\n<style lang=\"scss\" scoped>\n.form-precifique {\n  display: block;\n  width: 100%;\n  height: calc(1.5em + 0.75rem + 2px);\n  padding: 0.375rem 0.75rem;\n  font-size: 1rem;\n  font-weight: 400;\n  line-height: 1.5;\n  color: #495057;\n  background-color: #fff;\n  background-clip: padding-box;\n  border: 1px solid #ced4da;\n  border-radius: 0.25rem;\n  transition: border-color 0.15s ease-in-out, box-shadow 0.15s\n}\n\n.error {\n  color: $error;\n}\n\ninput:focus {\n  border-color: $primary;\n  /* Cor azul de foco (pode personalizar a cor) */\n  outline: none;\n  /* Remova a borda de contorno padrão que alguns navegadores adicionam */\n}\n</style>"]}]}