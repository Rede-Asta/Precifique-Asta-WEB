     <div class=\"my-close\" title=\"Fechar login\" @click=\"closeOptInModal()\">\n          <i class=\"fas fa-times\"></i>\n        </div>\n        <h3 class=\"text-center\">{{ title }}</h3>\n        <p class=\"m-0 text-muted text-center\">{{ subTitle }}</p>\n        <div\n          class=\"\n            d-flex\n            align-items-center\n            justify-content-between\n            col-8 col-md-6\n            mt-4\n            mb-0\n            p-0\n            opt-in-actions\n          \"\n        >\n          <button\n            type=\"button\"\n            class=\"\n              btn btn-link\n              text-muted\n              d-flex\n              flex-column\n              align-items-center\n              justify-content-center\n            \"\n            @click=\"sendEmailOrPhone('email')\"\n          >\n            <i class=\"far fa-envelope fa-3x\"></i>\n            <small>e-mail</small>\n          </button>\n          <small class=\"text-muted\">OU</small>\n          <button\n            type=\"button\"\n            class=\"\n              btn btn-link\n              text-muted\n              d-flex\n              flex-column\n              align-items-center\n              justify-content-center\n            \"\n            @click=\"sendEmailOrPhone('phone')\"\n          >\n            <img\n              class=\"whatsapp\"\n              src=\"@/assets/images/whatsapp-logo.svg\"\n              alt=\"WhatsApp\"\n            />\n            <small>whatsapp</small>\n          </button>\n        </div>\n      </div>\n    </div>\n  </modal>\n</template>\n\n<script lang=\"ts\">\n  import { Component, Prop, Vue } from \"vue-property-decorator\";\n\n  @Component({})\n  export class OptIn extends Vue {\n    @Prop() propShowOptIn!: boolean;\n\n    public showOptIn: boolean = this.propShowOptIn;\n    public title: string = \"Código de Validação\";\n    public subTitle: string =\n      \"Vamos te enviar um código de validação de 4 números. Como prefere receber?\";\n\n    public closeOptInModal() {\n      this.$emit(\"closeOptInModal\", true);\n    }\n\n    public sendEmailOrPhone(option: string) {\n      this.$emit(\"setOptIn\", option);\n      this.closeOptInModal();\n    }\n  }\n\n  // <opt-in\n  //     v-if=\"showOptIn\"\n  //     :propShowOptIn=\"showOptIn\"\n  //     v-on:closeOptInModal=\"closeOptInModal\"\n  //     v-on:setOptIn=\"setOptIn\"/>\n\n  export { OptIn as default };\n</script>\n\n<style lang=\"scss\" scoped>\n  @mixin select-item {\n    background-color: #f8f9fa;\n  }\n\n  .card-verification {\n    width: 25vw;\n    border-radius: 6px;\n    border: none;\n\n    @media screen and (max-width: 732px) {\n      width: 95vw;\n      height: auto;\n    }\n\n    @media screen and (min-width: 733px) and (max-width: 992px) {\n      width: 46vw;\n    }\n\n    @media screen and (min-width: 993px) and (max-width: 1200px) {\n      width: 34vw;\n    }\n\n    @media screen and (min-width: 1201px) and (max-width: 1400px) {\n      width: 30vw;\n    }\n\n    @media screen and (min-width: 1401px) and (max-width: 1600px) {\n      width: 32vw;\n    }\n  }\n\n  .my-close {\n    position: absolute;\n    top: -14px;\n    right: -14px;\n    background-color: $primary;\n    width: 40px;\n    height: 40px;\n    border-radius: 50%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    border: 3px solid #fff;\n    cursor: pointer;\n    transition: ease-in-out 1s;\n    z-index: 99999;\n\n    .fas {\n      color: #ffffff;\n    }\n\n    &:hover {\n      transition: ease-in-out 2s;\n      background-color: #ffffff;\n      .fas {\n        color: $primary;\n      }\n    }\n\n    @media screen and (max-width: 732px) {\n      top: 0px;\n      right: 0px;\n    }\n  }\n\n  .fa-envelope {\n    color: #ea4335;\n  }\n\n  .whatsapp {\n    width: 48px;\n    height: 48px;\n    cursor: pointer;\n  }\n\n  .btn-link {\n    height: 85px;\n\n    &:hover {\n      text-decoration: none;\n      @include select-item;\n    }\n  }\n\n  .opt-in-actions {\n    max-height: 100px;\n  }\n</style>"]}]}