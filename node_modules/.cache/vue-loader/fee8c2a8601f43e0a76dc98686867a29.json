c/components/register-seller/verification-code","sourcesContent":["<template>\n  <modal :active=\"showVerificationCode\">\n    <div class=\"card card-verification\">\n      <div\n        class=\"\n          card-body\n          d-flex\n          flex-column\n          align-items-center\n          justify-content-center\n        \"\n      >\n        <div\n          class=\"my-close\"\n          title=\"Fechar login\"\n          @click=\"closeVerificationCode()\"\n        >\n          <i class=\"fas fa-times\"></i>\n        </div>\n\n        <section\n          class=\"d-flex flex-column align-items-center justify-content-center\"\n        >\n          <h4 class=\"text-center\">Estamos felizes que você chegou!</h4>\n          <span\n            v-if=\"propIsRegister\"\n            class=\"\n              badge badge-user badge-light\n              mb-2\n              d-flex\n              flex-column\n              align-items-center\n              justify-content-center\n            \"\n          >\n            <p class=\"m-0 font-weight-bold\">\n              <i\n                :class=\"{\n                  'fab fa-whatsapp': optIn === 'phone',\n                  'far fa-envelope': optIn === 'email',\n                }\"\n              ></i>\n              {{ phone }}\n            </p>\n          </span>\n          <div v-if=\"propIsRegister\">\n            <button\n              class=\"btn btn-link d-flex align-items-center\"\n              style=\"height: 25px\"\n              @click=\"updateUserPhone()\"\n            >\n              <span class=\"material-icons\">edit_note</span>\n              <small>Editar telefone</small>\n            </button>\n          </div>\n          <section class=\"col-12\">\n            <div\n              class=\"\n                col-12\n                mb-2\n                p-0\n                d-flex\n                justify-content-center\n                align-items-end\n              \"\n            >\n              <small class=\"m-0 text-muted font-weight-bold text-center\">{{\n                title\n              }}</small>\n            </div>\n\n            <div\n              class=\"\n                row\n                col-10 col-lg-8\n                m-auto\n                p-0\n                code-wrapper\n                justify-content-center\n                alingn-items-center\n              \"\n            >\n              <div>\n                <input\n                  ref=\"codeInput1\"\n                  required\n                  maxLength=\"4\"\n                  class=\"code-input\"\n                  onClick=\"this.select();\"\n                  v-model=\"code\"\n                  type=\"tel\"\n                />\n              </div>\n            </div>\n          </section>\n          <div\n            class=\"\n              d-flex\n              flex-column\n              align-items-center\n              justify-content-center\n              mt-2\n              codigo\n            \"\n          >\n            <small class=\"text-muted\" v-if=\"countDown > 0\"\n              >Reenviar código\n            </small>\n            <div class=\"d-flex align-items-center justify-content-center\">\n              <h2 v-if=\"countDown > 0\">{{ minutes }}s</h2>\n              <button\n                class=\"btn btn-link d-flex align-items-center\"\n                @click=\"refreshCode()\"\n                v-if=\"countDown === 0\"\n              >\n                <span class=\"material-icons\" :class=\"{ 'fa-spin': refresh }\"\n                  >refresh</span\n                >\n                <small>Reenviar Código</small>\n              </button>\n            </div>\n          </div>\n          <div\n            class=\"\n              d-flex\n              flex-column\n              align-items-center\n              justify-content-center\n              col-12\n            \"\n          >\n            <button\n              class=\"btn btn-primary w-100\"\n              @click=\"submitCode($event)\"\n              ref=\"btnSubmit\"\n            >\n              Enviar\n            </button>\n          </div>\n        </section>\n      </div>\n    </div>\n  </modal>\n</template>\n\n<script lang=\"ts\">\n  import { Component, Prop, Vue } from \"vue-property-decorator\";\n\n  @Component({})\n  export class VerificationCode extends Vue {\n    @Prop() propShowVerificationCode!: boolean;\n    @Prop() propRefresh!: boolean;\n    @Prop() propBusinessName!: string;\n    @Prop() propEmail!: string;\n    @Prop() propPhone!: string;\n    @Prop() propOptIn!: string;\n    @Prop() propIsRegister!: boolean;\n\n    public showVerificationCode: boolean = this.propShowVerificationCode;\n    public refresh: boolean = this.propRefresh;\n    public businessName = this.propBusinessName;\n    public email: string = this.propEmail;\n    public phone: string = this.propPhone;\n    public optIn: string = this.propOptIn;\n\n    public title: string =\n      \"Insira abaixo o código de verificação que enviamos para o seu WhatsApp.\";\n    public code: string = \"\";\n    public countDown: number = 300;\n    public minutes: any = \"\";\n\n    created() {\n      this.countDownTimer();\n    }\n\n    public refreshCode() {\n      this.refresh = true;\n      this.$emit(\"refreshCodeConfirm\", true);\n      this.countDown = 300;\n      this.countDownTimer();\n    }\n\n    public submitCode() {\n      if (this.code) {\n        this.$emit(\"codeConfirm\", this.code);\n      }\n    }\n\n    private countDownTimer() {\n      if (this.countDown > 0) {\n        let duration = this.countDown;\n        let timer = duration,\n          minutes,\n          seconds;\n        setInterval(() => {\n          if (this.countDown > 0) {\n            minutes = parseInt(String(timer / 60), 10);\n            seconds = parseInt(String(timer % 60), 10);\n\n            minutes = minutes < 10 ? \"0\" + minutes : minutes;\n            seconds = seconds < 10 ? \"0\" + seconds : seconds;\n\n            this.minutes = minutes + \":\" + seconds;\n\n            if (--timer < 0 && this.countDown > 0) {\n              timer = duration;\n            }\n            this.countDown = timer;\n          }\n        }, 1000);\n      }\n    }\n\n    public closeVerificationCode() {\n      this.$emit(\"closeVerificationCode\", true);\n    }\n\n    public updateUserPhone() {\n      this.$emit(\"updateUserPhone\");\n    }\n  }\n\n  export { VerificationCode as default };\n</script>\n\n<style lang=\"scss\" scoped>\n  @mixin select-item {\n    background-color: #f8f9fa;\n  }\n\n  .card-verification {\n    width: 30vw;\n    border-radius: 6px;\n    border: none;\n\n    @media screen and (max-width: 732px) {\n      width: 95vw;\n      height: auto;\n    }\n    \n    @media screen and (min-width: 733px) and (max-width: 992px) {\n      width: 46vw;\n    }\n\n    @media screen and (min-width: 993px) and (max-width: 1200px) {\n      width: 34vw;\n    }\n\n    @media screen and (min-width: 1201px) and (max-width: 1400px) {\n      width: auto;\n    }\n\n    @media screen and (min-width: 1401px) and (max-width: 1600px) {\n      width: 32vw;\n    }\n  }\n\n  .my-close {\n    position: absolute;\n    top: -14px;\n    right: -14px;\n    background-color: $primary;\n    width: 40px;\n    height: 40px;\n    border-radius: 50%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    border: 3px solid #fff;\n    cursor: pointer;\n    transition: ease-in-out 1s;\n    z-index: 99999;\n\n    .fas {\n      color: #ffffff;\n    }\n\n    &:hover {\n      transition: ease-in-out 2s;\n      background-color: #ffffff;\n      \n      .fas {\n        color: $primary;\n      }\n    }\n\n    @media screen and (max-width: 992px) {\n      top: 0px;\n      right: 0px;\n    }\n  }\n\n  .fa-envelope {\n    color: #ea4335;\n  }\n\n  .whatsapp {\n    width: 48px;\n    height: 48px;\n    cursor: pointer;\n  }\n\n  .btn-link {\n    height: 85px;\n\n    &:hover {\n      text-decoration: none;\n      @include select-item;\n    }\n  }\n\n  .code-input {\n    width: 120px;\n    height: 50px;\n    text-align: center;\n    border-radius: 10px;\n    font-size: 24px;\n    letter-spacing: 5px;\n    align-self: center;\n    margin-top: 10px;\n  }\n\n  .btn-link {\n    &:hover,\n    &:focus {\n      text-decoration: none;\n    }\n  }\n\n  .codigo {\n    height: 100px;\n  }\n\n  .badge-user {\n    font-size: 16px;\n    border-radius: 6px;\n    padding: 10px;\n    font-weight: 400;\n    box-shadow: 0 5px 15px -5px rgba(43, 88, 164, 0.4);\n  }\n\n  .code-wrapper {\n    height: auto !important;\n  }\n</style>"]}]}