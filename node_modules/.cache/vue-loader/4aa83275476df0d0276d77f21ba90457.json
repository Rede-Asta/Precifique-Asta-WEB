tent":["<template>\n  <section id=\"step-five\" class=\"d-flex flex-column align-items-center justify-content-center\">\n    <p class=\"m-0 text-center\">Agora, acesse a área do vendedor e preencha as informações listadas abaixo para\n      proporcionar uma melhor experiência para seus clientes.</p>\n\n\n    <ul>\n      <li v-for=\"opt in arrayOpt\" @click=\"openEditBusiness(opt.type)\" class=\"d-flex align-items-center my-2 click\">\n        <div class=\"icons-img mr-2\">\n          <img :src=\"opt.img\" :alt=\"opt.name\" />\n        </div>\n        {{ opt.name }}\n      </li>\n    </ul>\n    <div>\n      <p class=\"text-center justify-content-center\">\n        Você receberá uma mensagem no seu WhatsApp com o menu de conteúdos, dicas e cursos gratuitos que\n        disponibilizamos para o seu negócio prosperar!\n      </p>\n    </div>\n\n\n    <router-link type=\"button\" class=\"btn btn-primary btn-action col-12 col-lg-6\" :to=\"{ path: '/dashboard' }\">Ir para\n      área do vendedor</router-link>\n\n    <modal :active=\"showEditBusiness\">\n      <section class=\"card card-business\" v-if=\"showEditBusiness\">\n        <div class=\"card-body\">\n          <div class=\"my-close-orders\" title=\"Fechar Modal\" @click=\"closeEditBusiness()\">\n            <i class=\"fas fa-times\"></i>\n          </div>\n\n          <editarMeuNegocio :propHeader=\"false\" :propActiveAccordion=\"activeAccordion\" />\n        </div>\n      </section>\n    </modal>\n  </section>\n</template>\n\n<script lang=\"ts\">\nimport { Component, Prop, Vue } from \"vue-property-decorator\";\n\n@Component({})\nexport class StepFive extends Vue {\n  public showEditBusiness: boolean = false;\n  public activeAccordion: string = \"\";\n  public arrayOpt: Array<{\n    type: string;\n    name: string;\n    img: string;\n  }> = [];\n\n  created() {\n    this.setArraOpt();\n  }\n\n  public setArraOpt() {\n    this.arrayOpt = [\n      {\n        type: \"payment\",\n        name: \"Formas de pagamento\",\n        img: require(`@/assets/images/icons_cadastro/payment.png`),\n      },\n      {\n        type: \"shipping\",\n        name: \"Frete ou retirada\",\n        img: require(`@/assets/images/icons_cadastro/shipping.png`),\n      },\n      {\n        type: \"products\",\n        name: \"Produtos\",\n        img: require(`@/assets/images/icons_cadastro/products.png`),\n      },\n      {\n        type: \"logo\",\n        name: \"Logomarca\",\n        img: require(`@/assets/images/icons_cadastro/logo.png`),\n      },\n      {\n        type: \"socialNetwork\",\n        name: \"Redes sociais\",\n        img: require(`@/assets/images/icons_cadastro/socialNetwork.png`),\n      },\n      {\n        type: \"officeHours\",\n        name: \"Horário de atendimento\",\n        img: require(\"@/assets/images/icons_cadastro/officeHours.png\"),\n      },\n    ];\n  }\n\n  public openEditBusiness(type: string) {\n    this.activeAccordion = type;\n    type !== \"products\"\n      ? (this.showEditBusiness = true)\n      : this.$router.push(\"produtos\");\n  }\n\n  public closeEditBusiness() {\n    this.showEditBusiness = false;\n  }\n}\n\nexport { StepFive as default };\n</script>\n\n<style lang=\"scss\" scoped>\n.icons-img {\n  width: 35px;\n  height: 35px;\n\n  img {\n    height: 30px;\n  }\n}\n\n.click {\n  cursor: pointer;\n}\n\n.card-business {\n  width: 65vw;\n  border-radius: 6px !important;\n  background-color: #f4f5f9;\n  border: none;\n\n  @media only screen and (max-width: 992px) {\n    width: 100vw;\n    height: 100vh !important;\n    border-radius: 0 !important;\n    overflow: auto;\n  }\n\n  @media only screen and (min-width: 992px) and (max-width: 1300px) {\n    width: 97vw;\n  }\n\n  @media only screen and (min-width: 1300px) and (max-width: 1600px) {\n    width: 88vw;\n  }\n\n  .card-body {\n    padding: 1rem !important;\n    min-height: auto !important;\n    max-height: 95vh;\n    overflow-y: auto;\n\n    @media only screen and (max-width: 992px) {\n      max-height: 100vh;\n    }\n  }\n\n  .card-titles {\n    margin-bottom: 1rem;\n\n    @media screen and (max-width: 992px) {\n      margin-bottom: 80px;\n    }\n  }\n\n  .card-title,\n  .card-subtitle {\n    font-family: Raleway;\n    font-stretch: normal;\n    font-style: normal;\n    line-height: normal;\n    letter-spacing: normal;\n    color: #525252;\n    text-align: center;\n  }\n\n  .card-title {\n    font-size: 28px;\n    font-weight: bold;\n\n    @media screen and (max-width: 992px) {\n      font-size: 26px;\n    }\n  }\n\n  .card-subtitle {\n    font-size: 20px;\n    font-weight: normal;\n    color: #525252;\n\n    @media screen and (max-width: 992px) {\n      font-size: 16px;\n    }\n  }\n\n  .card-action {\n    position: sticky;\n    bottom: -1rem;\n    background-color: #ffffff;\n    width: 100%;\n    height: 60px;\n    display: flex;\n    align-items: center;\n    margin: 30px 0 0 0;\n\n    @media screen and (max-width: 992px) {\n      margin: 30px 0;\n    }\n  }\n}\n\n.my-close-orders {\n  position: absolute;\n  top: -14px;\n  right: -14px;\n  background-color: #950ac6;\n  width: 40px;\n  height: 40px;\n  border-radius: 50%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border: 3px solid #fff;\n  cursor: pointer;\n  transition: ease-in-out 1s;\n  z-index: 99;\n\n  .fas {\n    color: #ffffff;\n  }\n\n  &:hover {\n    transition: ease-in-out 2s;\n    background-color: #ffffff;\n\n    .fas {\n      color: #950ac6;\n    }\n  }\n\n  @media screen and (max-width: 992px) {\n    top: 6px;\n    right: 6px;\n  }\n}\n\n.btn-action {\n  flex: 1;\n}\n</style>"]}]}