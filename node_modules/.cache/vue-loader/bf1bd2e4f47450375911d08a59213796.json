{"remainingRequest":"/home/aline/AstaPrecifique/node_modules/vue-loader/lib/index.js??vue-loader-options!/home/aline/AstaPrecifique/src/components/category-modal/category-delete/CategoryDelete.vue?vue&type=style&index=0&id=c11ec522&lang=scss&scoped=true&","dependencies":[{"path":"/home/aline/AstaPrecifique/src/components/category-modal/category-delete/CategoryDelete.vue","mtime":1686866940008},{"path":"/home/aline/AstaPrecifique/node_modules/css-loader/dist/cjs.js","mtime":1673370389401},{"path":"/home/aline/AstaPrecifique/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":1673370389594},{"path":"/home/aline/AstaPrecifique/node_modules/postcss-loader/src/index.js","mtime":1673370389569},{"path":"/home/aline/AstaPrecifique/node_modules/sass-loader/dist/cjs.js","mtime":1673370393532},{"path":"/home/aline/AstaPrecifique/node_modules/cache-loader/dist/cjs.js","mtime":1673370377252},{"path":"/home/aline/AstaPrecifique/node_modules/vue-loader/lib/index.js","mtime":1673370389594}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["CategoryDelete.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsHA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"CategoryDelete.vue","sourceRoot":"src/components/category-modal/category-delete","sourcesContent":["<template>\n  <modal :active=\"showDeleteCategory\">\n    <div class=\"card custom-modal\">\n      <div class=\"card-body text-center\">\n        <div\n          class=\"custom-close\"\n          title=\"Fechar Modal de Excluir\"\n          @click=\"closeDeleteCategoryModal()\"\n        >\n          <i class=\"fas fa-times\"></i>\n        </div>\n\n        <h4 class=\"font-weight-bold text-dark text-center text-uppercase\">\n          Excluir Categoria {{ categorySelected.name }}\n        </h4>\n        <p class=\"text-muted text-center mb-1\">\n          Tem certeza que deseja <span class=\"text-danger\">EXCLUIR</span> esta\n          categoria?\n        </p>\n        <small class=\"text-muted\"\n          >Deletando a categoria {{ categorySelected.name }} ela não estará mais\n          disponível, e todos os produtos associados a ela também serão\n          excluídos. Deseja continuar?</small\n        >\n\n        <div class=\"d-flex justify-content-between mt-4\">\n          <button class=\"btn btn-light\" @click=\"closeDeleteCategoryModal()\">\n            <i class=\"fas fa-times mr-2\"></i>\n            Cancelar\n          </button>\n          <button\n            class=\"btn btn-danger\"\n            @click=\"deleteCategory(categorySelected)\"\n          >\n            <i class=\"fas fa-trash mr-2\"></i>\n            Excluir Categoria\n          </button>\n        </div>\n      </div>\n    </div>\n  </modal>\n</template>\n\n<script lang=\"ts\">\n  import { Component, Prop, Vue } from \"vue-property-decorator\";\n  import VueSimpleAlert from \"vue-simple-alert\";\n  import Swal, { SweetAlertType } from \"sweetalert2\";\n\n  import categoryService from \"@/services/category.service\";\n  import { Category } from \"@/models/category\";\n\n  @Component({})\n  export class CategoryDelete extends Vue {\n    private categoryService = new categoryService();\n\n    @Prop() propCategorySelected!: Category;\n    @Prop() propShowDeleteCategory!: boolean;\n\n    public categorySelected: Category = this.propCategorySelected;\n    public showDeleteCategory: boolean = this.propShowDeleteCategory;\n\n    constructor() {\n      super();\n    }\n\n    public deleteCategory(category: Category) {\n      this.categoryService\n        .deleteCategory(category.guid)\n        .then(() =>\n          this.nextStatusSuccess(\n            `Pronto, a Categoria ${category.name} já foi excluída da sua vitrine.`\n          )\n        )\n        .catch(() =>\n          this.nextStatusError(\n            `Desculpe, ocorreu algum erro ao excluír a Categoria ${category.name}, por favor tente novamente mais tarde`\n          )\n        );\n    }\n\n    private nextStatusSuccess(msg: string) {\n      this.$emit(\"loading\", false);\n      this.$emit(\"getCategories\", true);\n      this.$emit(\"closeAllModal\", true);\n      this.closeDeleteCategoryModal();\n      this.toastMessage(\"success\", msg);\n    }\n\n    private nextStatusError(msg: string) {\n      this.$emit(\"loading\", false);\n      this.closeDeleteCategoryModal();\n      this.toastMessage(\"error\", msg);\n    }\n\n    public closeDeleteCategoryModal() {\n      this.$emit(\"closeDeleteCategoryModal\", true);\n    }\n\n    private toastMessage(type: SweetAlertType, title: string) {\n      VueSimpleAlert.fire({\n        type,\n        title,\n        toast: true,\n        position: \"top-end\",\n        showConfirmButton: false,\n        timer: 3000,\n        onOpen: (toast) => {\n          toast.addEventListener(\"mouseenter\", Swal.stopTimer),\n            toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\n        },\n      });\n    }\n  }\n\n  export { CategoryDelete as default };\n</script>\n\n<style lang=\"scss\" scoped>\n  .custom-modal {\n    width: 25vw;\n\n    .btn {\n      font-size: 14px;\n    }\n\n    .slider {\n      top: 6px !important;\n    }\n\n    .variant {\n      font-size: 14px;\n    }\n\n    @media screen and (max-width: 992px) {\n      width: 100vw;\n      height: 100vh;\n      border-radius: 0;\n\n      .btn {\n        font-size: 12px;\n      }\n\n      h4 {\n        margin-top: 20px;\n      }\n    }\n\n    @media screen and (min-width: 992px) and (max-width: 1360px) {\n      width: 40vw;\n    }\n  }\n</style>"]}]}